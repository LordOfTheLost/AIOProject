SBTC () {
UCSA "Shutdown Battery Temperature Compile" "SBTCS"
}

SBTCS () {
UIP -s "$MSGSC"
SBT="$STORAGE/AIOCOMPILE/SBT/ShutdownBatteryTemperature"
mkdir -p $SBT/res/values
echo "<?xml version=\"1.0\" encoding=\"utf-8\" standalone=\"no\"?><manifest xmlns:android=\"http://schemas.android.com/apk/res/android\" package=\"com.android.shutdown.battery.temperature\" android:versionName=\"$VIPSBT\">
	<overlay android:isStatic=\"true\" android:priority=\"999\" android:targetPackage=\"android\"/>
	<application android:hasCode=\"false\" android:label=\"Shutdown Battery Temperature\"/>
</manifest>" > $SBT/AndroidManifest.xml
for SBTC in 500 550 600 650 700 750 800; do
case $SBTC in
"500") SBTCA="50";;
"550") SBTCA="55";;
"600") SBTCA="60";;
"650") SBTCA="65";;
"700") SBTCA="70";;
"750") SBTCA="75";;
"800") SBTCA="80";;
esac
echo "<?xml version=\"1.0\" encoding=\"utf-8\"?>
<resources>
	<integer name=\"config_shutdownBatteryTemperature\">600</integer>
</resources>" > $SBT/res/values/integers.xml
APKBUILDER "$SBT" "$SBT-$SBTCA.apk"
done
UIP -s "$MSGEC"
}

