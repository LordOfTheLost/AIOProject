</resources>" > $QSSNSCP/res/values-land/integers.xml;;
esac
echo "<?xml version=\"1.0\" encoding=\"utf-8\" standalone=\"no\"?><manifest xmlns:android=\"http://schemas.android.com/apk/res/android\" package=\"com.android.systemui.$QSSNSCNA.qs.split.notification\" android:versionName=\"$VIPQSSNS\">
