IPBA() {
if [ $BOOTMODE != true ] && [ -f $AIOPFMDPH/bootanimation ] && [ -f $IPPM/bootanimation.zip.bak ]; then
MSGDELIMITER; UIP "$MSGFI Boot Animation, Remove?" "$MSGVU Yes" "$MSGVD No $MSGNUS"
if $KEYFUNCTION; then
UIP "Removing..."
BUCKUP="$IPPM/bootanimation.zip $IPPM/bootanimation-dark.zip"; BUCKUPER
rm -f $AIOPFMDPH/bootanimation
UIP "$MSGDRM"
fi
else
if [ $MIUI != true ] && $FINDAIOPFMDA; then
UCSA "Install Boot Animation $VIPBA?" "" "$MSGNUS" "IPBAS"
fi
fi
}

IPBAS() {
BATC=false
MSGDELIMITER; UIP "Choose Boot Animation:" "$MSGVU Google Pixel" "$MSGVD $MSGNIS"
if $KEYFUNCTION; then
MSGDELIMITER; UIP "Choose Color Variant:" "$MSGVU Black" "$MSGVD White"
if $KEYFUNCTION; then BADIR=GPBAB; else BADIR=GPBAW; fi; BATC=true
else
MSGDELIMITER; UIP "Choose Boot Animation:" "$MSGVU TenX" "$MSGVD $MSGNIS"
if $KEYFUNCTION; then
MSGDELIMITER; UIP "Choose Color Variant:" "$MSGVU Black" "$MSGVD White"
if $KEYFUNCTION; then BADIR=TENXB; else BADIR=TENXW; fi; BATC=true
else
MSGDELIMITER; UIP "Choose Boot Animation:" "$MSGVU PixelPlusUI" "$MSGVD $MSGNIS"
if $KEYFUNCTION; then
MSGDELIMITER; UIP "Choose Color Variant:" "$MSGVU Black" "$MSGVD White"
if $KEYFUNCTION; then BADIR=PPUIB; else BADIR=PPUIW; fi; BATC=true
else
MSGDELIMITER; UIP "Choose Boot Animation:" "$MSGVU Radiant" "$MSGVD $MSGNIS"
if $KEYFUNCTION; then
BATC=true; BADIR=RADIANT
else
MSGDELIMITER; UIP "Choose Boot Animation:" "$MSGVU OnePlus" "$MSGVD $MSGNUS"
if $KEYFUNCTION; then
BATC=true; BADIR=OPBM
fi
fi
fi
fi
fi
if $BATC; then
MSGDELIMITER; UIP "Choose FPS:" "$MSGVU 60" "$MSGVD 30"; if $KEYFUNCTION; then BAFPS="60"; else BAFPS="30"; fi
UIP "Installing Boot Animation $IAMM"
UZTPN "BA"
sed -i "s/#FPS/$BAFPS/g" $TMPDIR/BA/$BADIR/desc.txt
cd $TMPDIR/BA/$BADIR
$ZIPPACKER -r -0 $TMPDIR/BA/$BADIR * >/dev/null
mv -f $TMPDIR/BA/$BADIR.zip $TMPDIR/BA/bootanimation.zip
if $MAGISK; then
MODID="$IDBA"; REMOVER -mm "$MODID"
mkdir -p $MODPATH/$MODID$MIPPM
cp -f $TMPDIR/BA/bootanimation.zip $MODPATH/$MODID$MIPPM
if [ -f $IPPM/bootanimation-dark.zip ]; then cp -f $TMPDIR/BA/bootanimation.zip $MODPATH/$MODID$MIPPM/bootanimation-dark.zip; fi
MMPR -v "Boot Animation" "$VIPBA" "Boot Animation $FMR"; SET_PERM_RM
else
STANDARTBUCKUPER=true; BUCKUP="$IPPM/bootanimation.zip $IPPM/bootanimation-dark.zip"; BUCKUPER
cp -f $TMPDIR/BA/bootanimation.zip $IPPM; touch $AIOPFMDPH/bootanimation
if [ -f $IPPM/bootanimation-dark.zip ]; then cp -f $TMPDIR/BA/bootanimation.zip $IPPM/bootanimation-dark.zip; fi
fi
UIP "$MSGDI"
fi
}

