}

IPRUIS() {
UCSF "0dip 5dip 10dip 15dip 20dip 25dip 33dip" "Choose Rounded Size:" "Rounded Size" "33dip"
if $UCSFNM; then
UIP "Installing RoundedUI $IAMM"
UZTPN "UIROUNDED"
if $MAGISK; then
MODID="$IDRUI"; REMOVER -mm "$MODID"
mkdir -p $MODPATH/$MODID$MIPPSO
cp -f $TMPDIR/$MODID/Rounded*-$UCSFNMC.apk $MODPATH/$MODID$MIPPSO
MMPR -v "UI Rounded" "$VIPRUI" "UI Rounded Set On - $UCSFNMC, $FAOOR"; SET_PERM_RM
else
cp -f $TMPDIR/UIROUNDED/Rounded*-$UCSFNMC.apk $IPPSO; touch $AIOPFMDPH/roundedui
fi
UIP "$MSGDI"
fi
}

