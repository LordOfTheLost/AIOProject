PXMLNLS -s $DEVFEAA '/features/bool[@name="support_samplerate_96000"]' "true"
PXMLNLS -s $DEVFEAA '/features/bool[@name="support_samplerate_192000"]' "true"
PXMLNLS -s $DEVFEAA '/features/bool[@name="support_low_latency"]' "true"
PXMLNLS -s $DEVFEAA '/features/bool[@name="support_mid_latency"]' "false"
PXMLNLS -s $DEVFEAA '/features/bool[@name="support_high_latency"]' "false"
PXMLNLS -s $DEVFEAA '/features/bool[@name="support_interview_record_param"]' "false"
PXMLNLS -s $DEVFEAA '/features/bool[@name="support_voip_record"]' "true"
PXMLNLS -s $DEVFEAA '/features/integer[@name="support_inner_record"]' "1"
PXMLNLS -s $DEVFEAA '/features/bool[@name="support_hifi"]' "true"
UIP "Files Found And Patched"
done
}

PATCH_DIRAC() {
UIP "Patching Needed Files..."
for OFILE in $CFGS; do
UIP "Searching In: $OFILE"
FILE="$MODPATH/$MODID$(echo $OFILE | sed "s|^/vendor|$MIPSV|g")"
mkdir -p `dirname $FILE`
cp -f $MAGISKMIRROR$OFILE $FILE
OSP_DETECT $FILE
ALBP "dirac_gef" "$MODID" "$DYNLIBPATCH\/lib\/soundfx" "libdiraceffect.so" "3799D6D1-22C5-43C3-B3EC-D664CF8D2F0D"
PROCESSING_PATCH -post "$FILE" "music" "dirac_gef"
UIP "Files Found And Patched"
done
mkdir -p $MODPATH/$MODID$MIPSVE/dirac $MODPATH/$MODID$MIPSVL/rfsa/adsp $MODPATH/$MODID$MIPSVLS
cp -f $NEWDIRAC/diracvdd.bin $MODPATH/$MODID$MIPSVE
cp -f $NEWDIRAC/interfacedb $MODPATH/$MODID$MIPSVE/dirac
cp -f $NEWDIRAC/dirac_resource.dar $MODPATH/$MODID$MIPSVL/rfsa/adsp
cp -f $NEWDIRAC/libdirac-capiv2.so $MODPATH/$MODID$MIPSVL/rfsa/adsp/dirac.so
cp -f $NEWDIRAC/libdirac-capiv2.so $MODPATH/$MODID$MIPSVL/rfsa/adsp
cp -f $NEWDIRAC/libdiraceffect.so $MODPATH/$MODID$MIPSVLS
echo -e "\n# Patch Dirac
persist.dirac.acs.controller=gef
persist.dirac.gef.oppo.syss=true
persist.dirac.config=64
persist.dirac.gef.exs.did=29,49
persist.dirac.gef.ext.did=10,20,29,49
persist.dirac.gef.ins.did=19,134,150
persist.dirac.gef.int.did=15,19,134,150
persist.dirac.gef.ext.appt=0x00011130,0x00011134,0x00011136
persist.dirac.gef.exs.appt=0x00011130,0x00011131
persist.dirac.gef.int.appt=0x00011130,0x00011134,0x00011136
persist.dirac.gef.ins.appt=0x00011130,0x00011131
persist.dirac.gef.exs.mid=268512739
persist.dirac.gef.ext.mid=268512737
persist.dirac.gef.ins.mid=268512738
persist.dirac.gef.int.mid=268512736
persist.dirac.path=/vendor/etc/dirac
ro.dirac.acs.storeSettings=1
persist.dirac.acs.ignore_error=1" >> $MODPATH/$MODID/system.prop
}

PATCH_MIXER() {
UIP "Patching Needed Files..."
for OMIX in $MPATHS; do
UIP "Searching In: $OMIX"
MIX="$MODPATH/$MODID$(echo $OMIX | sed "s|^/vendor|$MIPSV|g")"
if $HIFI; then
PXMLNLS -u $MIX '/mixer/ctl[@name="RX_HPH_PWR_MODE"]' "HIFI"
PXMLNLS -u $MIX '/mixer/ctl[@name="RX HPH Mode"]' "CLS_H_HIFI"
PXMLNLS -u $MIX '/mixer/path[@name="headphones"]/ctl[@name="RX HPH Mode"]' "CLS_H_HIFI"
PXMLNLS -s $MIX '/mixer/path[@name="headphones"]/ctl[@name="RX HPH Mode"]' "CLS_H_HIFI"
PXMLNLS -s $MIX '/mixer/path[@name="headphones-44.1"]/ctl[@name="RX HPH Mode"]' "CLS_H_HIFI"
PXMLNLS -u $MIX '/mixer/path[@name="headphones-dsd"]/ctl[@name="RX HPH Mode"]' "CLS_H_HIFI"
PXMLNLS -u $MIX '/mixer/path[@name="hph-hifi-mode"]/ctl[@name="RX HPH Mode"]' "CLS_H_HIFI"
PXMLNLS -u $MIX '/mixer/path[@name="hph-highquality-mode"]/ctl[@name="RX HPH Mode"]' "CLS_H_HIFI"
PXMLNLS -u $MIX '/mixer/path[@name="hph-lowpower-mode"]/ctl[@name="RX HPH Mode"]' "CLS_H_HIFI"
PXMLNLS -u $MIX '/mixer/path[@name="hph-class-ab-mode"]/ctl[@name="RX HPH Mode"]' "CLS_H_HIFI"
PXMLNLS -s $MIX '/mixer/path[@name="hph-class-ab-mode"]/ctl[@name="RX HPH Mode"]' "CLS_H_HIFI"
PXMLNLS -u $MIX '/mixer/path[@name="headphones"]/ctl[@name="RX_HPH_PWR_MODE"]' "HIFI"
PXMLNLS -s $MIX '/mixer/path[@name="headphones"]/ctl[@name="RX_HPH_PWR_MODE"]' "HIFI"
PXMLNLS -s $MIX '/mixer/path[@name="headphones-44.1"]/ctl[@name="RX_HPH_PWR_MODE"]' "HIFI"
PXMLNLS -s $MIX '/mixer/path[@name="headphones-dsd"]/ctl[@name="RX_HPH_PWR_MODE"]' "HIFI"
PXMLNLS -u $MIX '/mixer/path[@name="hph-hifi-mode"]/ctl[@name="RX_HPH_PWR_MODE"]' "HIFI"
PXMLNLS -s $MIX '/mixer/path[@name="hph-hifi-mode"]/ctl[@name="RX_HPH_PWR_MODE"]' "HIFI"
PXMLNLS -u $MIX '/mixer/path[@name="hph-highquality-mode"]/ctl[@name="RX_HPH_PWR_MODE"]' "HIFI"
PXMLNLS -s $MIX '/mixer/path[@name="hph-highquality-mode"]/ctl[@name="RX_HPH_PWR_MODE"]' "HIFI"
PXMLNLS -u $MIX '/mixer/path[@name="hph-lowpower-mode"]/ctl[@name="RX_HPH_PWR_MODE"]' "HIFI"
PXMLNLS -s $MIX '/mixer/path[@name="hph-lowpower-mode"]/ctl[@name="RX_HPH_PWR_MODE"]' "HIFI"
PXMLNLS -u $MIX '/mixer/path[@name="hph-class-ab-mode"]/ctl[@name="RX_HPH_PWR_MODE"]' "HIFI"
PXMLNLS -s $MIX '/mixer/path[@name="hph-class-ab-mode"]/ctl[@name="RX_HPH_PWR_MODE"]' "HIFI"
else
PXMLNLS -s $MIX '/mixer/ctl[@name="RX_HPH_PWR_MODE"]' "HIRES"
PXMLNLS -u $MIX '/mixer/ctl[@name="RX HPH Mode"]' "HD2"
PXMLNLS -s $MIX '/mixer/path[@name="headphones"]/ctl[@name="RX_HPH_PWR_MODE"]' "HIRES"
fi
if [ "$RN5PRO" ] || [ "$MI9" ] || [ "$MI8" ] || [ "$MI8P" ] || [ "$MI9P" ] || [ "$MIA2" ]; then
PXMLNLS -s $MIX '/mixer/path[@name="headphones-44.1"]/ctl[@name="SLIM_5_RX Format"]' "S24_3LE"
PXMLNLS -s $MIX '/mixer/ctl[@name="TAS2557 ClassD Edge"]' "7"
PXMLNLS -s $MIX '/mixer/ctl[@name="TAS2557 Volume"]' "30"
fi
if $PATCH_MIXER_LITE; then
PXMLNLS -s $MIX '/mixer/ctl[@name="headphones"]/ctl[@name="SLIM_5_RX Format"]' "S24_3LE"
PXMLNLS -s $MIX '/mixer/ctl[@name="headphones"]/ctl[@name="PowerCtrl"]' "1"
