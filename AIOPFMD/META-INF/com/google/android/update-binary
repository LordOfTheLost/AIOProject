#!/sbin/sh

{ #Default Var
set -x
VCAIOPFMD="110"; VAIOPFMD="31.03.24 ($VCAIOPFMD)"
IPDLTMP="/data/local/tmp"
TMPDIR="$IPDLTMP/AIOPFMD"
DEVTMPDIR="/dev/AIOPFMD"
IPDADB="/data/adb"
IPDMM="/data/magisk"
