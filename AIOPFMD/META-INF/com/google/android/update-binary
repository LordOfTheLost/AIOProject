    <application android:extractNativeLibs=\"false\"/>
    <overlay android:isStatic=\"true\" android:priority=\"0\" android:targetPackage=\"$FULS\" android:label=\"IR FaceUnlock Service\"/>
</manifest>" > $OFULS/AndroidManifest.xml

echo "<?xml version=\"1.0\" encoding=\"utf-8\"?>
<resources>
    <integer name=\"override_front_cam_id\">$SFULS</integer>
</resources>" > $OFULS/res/values/integers.xml

case $SFULS in
"5") SFULS="On";;
"-1") SFULS="Off";;
esac

APKBUILDER "$OFULS" "$OFULS-$FULS-$SFULS.apk"
done
done
}

