{ #UI: LockScreen/AlwaysOnDisplay

IPQRCSOLS () {
if FINDER -aiopfmdph "$IPPSO/QRCodeScannerOnLockScreen*.apk"; then
UIP -s "$MSGFI QR Code Scanner On Lock Screen, Remove?" "$MSGVU Yes" "$MSGVD No $MSGNUS"
if KEYCHOOSE; then
