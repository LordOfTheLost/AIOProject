PXMLNLS -s $MIX '/mixer/ctl[@name="HPH Idle Detect"]' "ON"
PXMLNLS -s $MIX '/mixer/ctl[@name="Set Custom Stereo OnOff"]' "1"
PXMLNLS -s $MIX '/mixer/ctl[@name="HiFi Function"]' "On"
