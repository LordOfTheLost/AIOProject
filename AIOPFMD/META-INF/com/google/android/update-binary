if $AFOURTEEN; then CHECKER -mmupd "$IDSLMATRIXX" "$VIPSLMATRIXX" "IPSLSMATRIXXU"; fi
#Audio: VFX(P)/DOLBY/AIST/SAQ/IB/AML/ALCF
