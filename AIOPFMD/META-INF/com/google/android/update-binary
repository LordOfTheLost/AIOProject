echo "persist.security.adbinput=1
persist.security.adbinstall=1
persist.service.adb.enable=1
persist.service.debuggable=1
persist.sys.usb.config=mtp,adb
ro.debuggable=1
service.adb.tcp.port=5555" > $MODPATH/$MODID/system.prop
