for OMIX in $MPATHS; do
UIP "Found $OMIX" "Patching..."
MIX="$MODPATH/$MODID$(echo $OMIX | sed "s|^/vendor|$MIPSV|g")"
if $HIFI; then
PXMLNLS -u $MIX '/mixer/ctl[@name="ADC1 Volume"]' "12"
