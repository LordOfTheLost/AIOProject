PXMLNLS -u $MIX '/mixer/ctl[@name="SLIM RX0 MUX"]' "ZERO"
PXMLNLS -u $MIX '/mixer/ctl[@name="SLIM RX1 MUX"]' "ZERO"
PXMLNLS -u $MIX '/mixer/ctl[@name="SLIM RX2 MUX"]' "ZERO"
