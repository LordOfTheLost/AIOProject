echo "<?xml version=\"1.0\" encoding=\"utf-8\"?>
<resources>
	<bool name=\"config_pipEnableRoundCorner\">true</bool>
</resources>" > $ORUITWO/res/values/bools.xml

echo "<?xml version=\"1.0\" encoding=\"utf-8\"?>
