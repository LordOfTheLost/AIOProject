CHECKER -upd "$IDAS" "$VIPAS" "$VCIPAS" "IPASS"
#Launchers: Shady/Lawnchair/Sushi/Ortus
if $ATEN; then CHECKER -upd "$IDSLSHADY" "$VIPSLSHADYQ" "$VCIPSLSHADYQ" "IPSLSSHADY"; fi
if $AELEVEN; then CHECKER -upd "$IDSLSHADY" "$VIPSLSHADYR" "$VCIPSLSHADYR" "IPSLSSHADY"; fi
if $ATEN || $AELEVEN || $ATWELVEL || $ATHIRTEEN || $AFOURTEEN; then CHECKER -upd "$IDSLLAWN" "$VIPSLLAWN" "$VCIPSLLAWN" "IPSLLAWNA"; fi
if $ATWELVEL; then CHECKER -upd "$IDSLSUSHI" "$VIPSLSUSHI" "$VCIPSLSUSHI" "IPSLSSUSHISL"; fi
if $AFOURTEEN; then CHECKER -upd "$IDSLORTUS" "$VIPSLORTUS" "$VCIPSLORTUS" "IPSLSORTUSU"; fi
#Audio: VFX(P)/DOLBY/AIST/SAQ/SIB/AML/ALCF/VS
