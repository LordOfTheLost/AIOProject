}

CINNICS() {
for VACINN in "20dip" "30dip" "40dip" "50dip" "60dip" "70dip"; do
case $VACINN in
"20dip") NACINN="a";;
"30dip") NACINN="b";;
"40dip") NACINN="c";;
"50dip") NACINN="d";;
"60dip") NACINN="e";;
"70dip") NACINN="f";;
esac
OCINNI="$TMPDIR/CINNI/Framework_Increase_Number_Notification_Icons"
mkdir -p $OCINNI/res/values

echo "<?xml version=\"1.0\" encoding=\"utf-8\" standalone=\"no\"?><manifest xmlns:android=\"http://schemas.android.com/apk/res/android\" package=\"com.android.increase.number.notification.icons.$NACINN\">
    <overlay android:category=\"com.android.internal.display_cutout_emulation\" android:priority=\"999\" android:targetPackage=\"android\"/>
    <application android:hasCode=\"false\" android:label=\"Android Increase Number Notification Icons $VACINN\"/>
