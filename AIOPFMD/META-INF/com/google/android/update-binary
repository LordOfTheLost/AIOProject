}

PRESETS() {
if [ -d $MODPATH ] && [ $MAGISKFIRSTTIME != true ] && $MAGISK && $MMUPDC; then for mmc in $(ls $MODPATH); do if [ -d "$MODPATH/$mmc" ]; then UCSA "Checking Update?" "$MSGEFTC" "$MSGNUS" "MMUPD_LIST" && break; fi; done; fi
if $NEWDESIGN; then
MSGDELIMITER; UIP "UI: Pills/NavBar/Gboard Themes" "$MSGVU Yes $MSGEFTC" "$MSGVD No $MSGNUS"; if $KEYFUNCTION; then UI_PRESETS; fi
if $MIUI && [ -d $STORAGE/Applications ]; then IPAPP; else MSGDELIMITER; UIP "Apps: GFU/GSF/SL/IA" "$MSGVU Yes $MSGEFTC" "$MSGVD No $MSGNUS"; if $KEYFUNCTION; then APPS_PRESETS; fi; fi
MSGDELIMITER; UIP "Audio: VFX(P)/DSP/DTS/AN/ACP/AML" "$MSGVU Yes $MSGEFTC" "$MSGVD No $MSGNUS"; if $KEYFUNCTION; then AUDIO_PRESETS; fi
if $BOOTMODE; then REMOVAL_PRESETS; elif [ $BOOTMODE != true ]; then MSGDELIMITER; UIP "Removal: RSL/RSO/RSTA/RWFSB" "$MSGVU Yes $MSGEFTC" "$MSGVD No $MSGNUS"; if $KEYFUNCTION; then REMOVAL_PRESETS; fi; fi
if $TESTMODE; then MSGDELIMITER; UIP "Test/Coming Soon" "$MSGVU Yes $MSGEFTC" "$MSGVD No $MSGNUS"; if $KEYFUNCTION; then TEST_COMING_SOON_PRESETS; fi; fi
MSGDELIMITER; UIP "Net: GPS/TTL/DNS/WB" "$MSGVU Yes $MSGEFTC" "$MSGVD No $MSGNUS"; if $KEYFUNCTION; then NET_PRESETS; fi
MSGDELIMITER; UIP "Utilities: BB/ZSigner/SELC/ASTE/CC" "$MSGVU Yes $MSGEFTC" "$MSGVD No $MSGNUS"; if $KEYFUNCTION; then UTILITIES_PRESETS; fi
MSGDELIMITER; UIP "Other: DFO/STZD/DDVFE/FA/SBU" "$MSGVU Yes $MSGEFTC" "$MSGVD No $MSGNUS"; if $KEYFUNCTION; then OTHER_PRESETS; fi
IPHF
if $MAGISK; then MSGDELIMITER; UIP "SafetyNet/MHPC/GPay" "$MSGVU Yes $MSGEFTC" "$MSGVD No $MSGNUS"; if $KEYFUNCTION; then SAFETY_NET_AND_MHPC_PRESETS; fi; if $AOREO || $APIE || $ATEN || $AELEVEN; then MSGDELIMITER; UIP "Riru/Edx/LS/Posed/Mudules" "$MSGVU Yes $MSGEFTC" "$MSGVD No $MSGNUS"; if $KEYFUNCTION; then RIRU_AND_EDX_LS_POSED_PRESETS; fi; fi; fi
if [ $ENDMODE != true ]; then MSGNUS="$MSGEND"; fi
MSGDELIMITER; UIP "Tweaks: FGS/BP/LED/UGMSD/USGP/VS" "$MSGVU Yes $MSGEFTC" "$MSGVD No $MSGNUS"; if $KEYFUNCTION; then MSGNUS="(/)"; TWEAKS_PRESETS; fi
elif $OLDDESIGN; then
UI_PRESETS
APPS_PRESETS
AUDIO_PRESETS
REMOVAL_PRESETS
TEST_COMING_SOON_PRESETS
NET_PRESETS
UTILITIES_PRESETS
OTHER_PRESETS
IPHF
SAFETY_NET_AND_MHPC_PRESETS
RIRU_AND_EDX_LS_POSED_PRESETS
TWEAKS_PRESETS
fi
}

} #

