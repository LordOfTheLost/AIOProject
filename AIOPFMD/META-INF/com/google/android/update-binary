if $BOOTMODE; then UIP "Wi-Fi & Mobile Data QS Old Style Activated Right Now"; $SPGLOBAL settings_provider_model false; $SPSECURE sysui_qs_tiles "wifi,cell,$($SGSECURE sysui_qs_tiles)"; fi
MMPR -v "Wi-Fi & Mobile Data QS Old Style" "$VIPGPPM" "Wi-Fi & Mobile Data QS Old Style"; SET_PERM_RM -msgdi
}

IPQRCSOLS () {
if FINDER -aiopfmdph "$IPPSO/QRCodeScannerOnLockScreen*.apk"; then
UIP -s "$MSGFI QR Code Scanner On Lock Screen, Remove?" "$MSGVU Yes" "$MSGVD No $MSGNUS"
if KEYCHOOSE; then
UIP "Removing..."
rm -f $IPPSO/QRCodeScannerOnLockScreen*.apk
UIP "$MSGDRM"
fi
else
if [ $MIUI != true ] && [ "$ASDK" -ge 33 ]; then UCSA -ent "$MSGIP QR Code Scanner On Lock Screen $VIPQRCSOLS?" "IPQRCSOLSS" "$IDQRCSOLS"; fi
fi
}

IPQRCSOLSS () {
UIP -s "Choose Variant:" "$MSGVU Enable" "$MSGVD Disable"
if KEYCHOOSE; then QRCSOLSV="Enable"; QRCSOLSVO="1"; QRCSOLSVT="0"; else QRCSOLSV="Disable"; QRCSOLSVO="0"; QRCSOLSVT="1"; fi
UIP "Installing QR Code Scanner On Lock Screen $IAMM"
UZTPN "QRCSOLS"
if $MAGISK; then
PTMFI -nm "$IDQRCSOLS" "$MIPPSO"
cp -f $TMPDIR/QRCSOLS/QRCodeScannerOnLockScreen-$QRCSOLSV.apk $MODPATH/$MODID$MIPPSO
if CHECKER -sp "$XMLSECURE" "lock_screen_show_qr_code_scanner"; then
MSSERVICE "$SPSECURE lock_screen_show_qr_code_scanner $QRCSOLSVT" "$SPSECURE lock_screen_show_qr_code_scanner $QRCSOLSVO" "QRCSOLS"
fi
MMPR -v "QR Code Scanner On Lock Screen" "$VIPQRCSOLS" "QR Code Scanner On Lock Screen - Set On: $QRCSOLSV"
else
cp -f $TMPDIR/QRCSOLS/QRCodeScannerOnLockScreen-$QRCSOLSV.apk $IPPSO
fi
SET_PERM_RM -msgdi
}

