}

QCDFP () {
for f in $EQCDFP; do
if [ -f $f ]; then
if grep -m1 "^$1" $f >/dev/null; then
local g=$(grep -m1 "^$1" $f)
sed -i "s~$g~$1=$2~g" $f
elif grep -m1 "^#$1" $f >/dev/null; then
local g=$(grep -m1 "^#$1" $f)
sed -i "s~$g~$1=$2~g" $f
elif grep -m1 "^# $1" $f >/dev/null; then
local g=$(grep -m1 "^# $1" $f)
sed -i "s~$g~$1=$2~g" $f
else
echo -e "\n$3\n$1=$2" >> $f
fi
fi
done
}

PTMFI () {
case "$1" in
-nm) MODID="$2"; local IP="$3"; REMOVER -mm "$MODID"; for MP in $IP; do mkdir -p "$MODPATH/$MODID$MP"; done;;
*) MODID="$1"; local DMM="$2"; local IP="$3"; REMOVER -mm "$MODID $DMM"; mkdir -p "$MODPATH/$MODID"; for MP in $IP; do mkdir -p "$MODPATH/$MODID$MP"; done;;
esac
touch $MODPATH/$MODID/update
}

UNIVERSALUNZIP () {
UZTPN "$1"
cd $TMPDIR/$1 && $ZIPPACKER -r -0 $TMPDIR/$1 * >/dev/null
OFPATHZIP="$TMPDIR/$1"
mkdir -p $OFPATHZIP
$UNZIP -oq "$ZIPFILE" '$OFPATHZIP.zip' -d "$OFPATHZIP"
$UNZIP -oq "$OFPATHZIP.zip" 'META-INF/com/google/android/update-binary' -d "$OFPATHZIP"
chmod 0755 "$OFPATHZIP/META-INF/com/google/android/update-binary"
sh $OFPATHZIP/META-INF/com/google/android/update-binary \
"$IN" "$OUTFD" "$OFPATHZIP.zip"
rm -rf $OFPATHZIP*
if [ $BOOTMODE != true ]; then UIP -s "ReMounting$MOUNTS"; PARTITION_AND_MOUNTS; fi
}

} #

{ #KeyChoose

KEYCHOOSE () {
while true; do
eval $(getevent -lqc 1 | awk '{print "K=" $3; print "V=" $4}')
if [[ "$K" = "KEY_VOLUMEUP" && "$V" = "DOWN" ]]; then
HFBMODE && return 0
elif [[ "$K" = "KEY_VOLUMEDOWN" && "$V" = "DOWN" ]]; then
HFBMODE && return 1
elif $GESTURES && [[ "$K" = "ABS_MT_SLOT" && "$V" = "00000000" ]]; then
HFBMODE && END
fi
done
}

} #

{ #All Otval

{ #UI: Pills/NavigationBar/Gestures

IPP () {
if FINDER -aiopfmdph "$IPPSO/Pills.apk"; then
UIP -s "$MSGFI Pills, Remove?" "$MSGVU Yes" "$MSGVD No $MSGNUS"
if KEYCHOOSE; then
UIP "$MSGRMIP"
rm -f $IPPSO/Pills.apk $IPPSO/NavigationBarModeGesturalOverlay
UIP "$MSGDRM"
fi
else
if [ $MIUI != true ] && [ "$ASDK" -ge 29 ]; then UCSA -ent "$MSGIP Pills $VIPP?" "IPPS" "$IDP"; fi
fi
}

IPPS () {
PILLSHIDE=false
IMMERSIVE=false
FIMMERSIVE=false
UIP -s "Choose Variant Pills:" "$MSGVU Full Hide/Immersive" "$MSGVD More Other Variants"
if KEYCHOOSE; then
PILLSHIDE=true; PFSGVFPX=true; POFSGC=true; FIMMERSIVE=true; PILLSWIDTHSIZE="0dip"; PILLSFATSIZE="0dip"
else
UIP -s "Transparent Variant:" "$MSGVU Transparent" "$MSGVD Not Transparent"
if KEYCHOOSE; then IMMERSIVE=true; fi
UIP -s "Choose Width Size:" "$MSGVU 4dip/8dip (Dot)" "$MSGVD No $MSGNIS"
if KEYCHOOSE; then
POFSGCDOT=true; PHOC=true; PILLSFATSIZE="4dip"; PILLSWIDTHSIZE="8dip"
else
POFSGCDOT=false
UIP -s "$MSGVU 0dip/0dip (Hide)" "$MSGVD No $MSGNIS"
if KEYCHOOSE; then
POFSGC=true; PHOC=true; PILLSFATSIZE="0dip"; PILLSWIDTHSIZE="0dip"
else
PHOC=false
for PILLSWIDTHSIZE in "60dip" "120dip" "180dip" "240dip" "300dip"; do
UIP -s "$MSGVU $PILLSWIDTHSIZE"
MSGPOFSGA="$MSGNIS"
case $PILLSWIDTHSIZE in
"300dip") MSGPOFSGA="$MSGNUS";;
esac
UIP "$MSGVD No $MSGPOFSGA"
if KEYCHOOSE; then
POFSGC=true
break
else
POFSGC=false
fi
done
fi
fi
if $POFSGC && [ $POFSGCDOT != true ] || [ $PHOC != true ]; then
if $POFSGC && [ $PHOC != true ]; then
MSGPOFSG=true
for PILLSFATSIZE in "1dip" "2dip" "3dip" "4dip"; do
if $MSGPOFSG; then
UIP -s "Choose Fat Size:"
MSGPOFSG=false
else
MSGCS
fi
UIP "$MSGVU $PILLSFATSIZE"
MSGPOFSGA="$MSGNIS"
case $PILLSFATSIZE in
"4.0dp") MSGPOFSGA="$MSGNUS";;
esac
UIP "$MSGVD No $MSGPOFSGA"
if KEYCHOOSE; then
PFSGVFPX=true
break
else
PFSGVFPX=false
fi
done
fi
fi
fi
if $POFSGC && $PFSGVFPX; then
UIP "$MSGIAMM Pills"
UZTPN "Pills"
if $MAGISK; then
PTMFI "$IDP" "$IDAP" "$MIPPSO"
if $IMMERSIVE; then
cp -f $TMPDIR/$MODID/PillsImmersive-48.0dip.apk $MODPATH/$MODID$MIPPSO
fi
if $FIMMERSIVE; then
cp -f $TMPDIR/$MODID/PillsImmersive-0.0dip.apk $MODPATH/$MODID$MIPPSO
fi
cp -f $TMPDIR/$MODID/Pills-$PILLSFATSIZE-$PILLSWIDTHSIZE.apk $MODPATH/$MODID$MIPPSO
if $PILLSHIDE; then
MMPR -v "Pills" "$VIPP" "$VCIPP" "Pills Full Hide/Immersive"
else
MMPR -v "Pills" "$VIPP" "$VCIPP" "Pills Size $PILLSFATSIZE/$PILLSWIDTHSIZE"
fi
else
if $IMMERSIVE; then
cp -f $TMPDIR/Pills/PillsImmersive-48.0dip.apk $IPPSO
fi
if $FIMMERSIVE; then
cp -f $TMPDIR/Pills/PillsImmersive-0.0dip.apk $IPPSO
fi
cp -f $TMPDIR/Pills/Pills-$PILLSFATSIZE-$PILLSWIDTHSIZE.apk $IPPSO/Pills.apk
fi
SET_PERM_RM -msgdi
fi
}

IPPC () {
if FINDER -aiopfmdph "$IPPSO/PillsColors*.apk"; then
UIP -s "$MSGFI Pills Colors, Remove?" "$MSGVU Yes" "$MSGVD No $MSGNUS"
if KEYCHOOSE; then
UIP "$MSGRMIP"
rm -f $IPPSO/PillsColors*.apk
UIP "$MSGDRM"
fi
else
if [ $MIUI != true ] && [ "$ASDK" -ge 29 ]; then UCSA -ent "$MSGIP Pills Colors $VIPPC?" "IPPCS" "$IDPC"; fi
fi
}

IPPCS () {
UIP -s "Choose Variant:" "$MSGVU Choose One Color" "$MSGVD Choose Two Color Separately"
if KEYCHOOSE; then COLORVARIANT=" "; IPPCAS; PILLSLIGHTCOLORS=$PILLSCOLORS; PILLSDARKCOLORS=$PILLSCOLORS; else COLORVARIANT=" Light: "; IPPCOAS; if $PCOFSGC; then PILLSLIGHTCOLORS=$PILLSCOLORS; COLORVARIANT=" Dark: "; IPPCOAS; PILLSDARKCOLORS=$PILLSCOLORS; fi; fi
if $PCOFSGC; then
UIP "$MSGIAMM Pills Colors"
UZTPN "PillsColors"
if $MAGISK; then
PTMFI "$IDPC" "$IDAPC" "$MIPPSO"
cp -f $TMPDIR/$MODID/PillsColors-$PILLSLIGHTCOLORS-$PILLSDARKCOLORS.apk $MODPATH/$MODID$MIPPSO
MMPR -v "Pills Colors" "$VIPPC" "$VCIPPC" "Pills Colors - Set On: Light - $PILLSLIGHTCOLORS & Dark - $PILLSDARKCOLORS"
else
cp -f $TMPDIR/PillsColors/PillsColors-$PILLSLIGHTCOLORS-$PILLSDARKCOLORS.apk $IPPSO
fi
SET_PERM_RM -msgdi
fi
}

IPPCAS () {
for PILLSCOLORS in "Accent" "ff1a73e8" "ff5e97f6" "ffb31818" "ffff4151" "ff1ed760" "ff47ae84" "ffffa842" "ffff8055" "ffa86bd5"; do
case $PILLSCOLORS in
"Accent") NAMEPILLSCOLORS="System Accent";;
"ff1a73e8") NAMEPILLSCOLORS="Blue";;
"ff5e97f6") NAMEPILLSCOLORS="Light Blue";;
"ffb31818") NAMEPILLSCOLORS="Red";;
"ffff4151") NAMEPILLSCOLORS="Light Red";;
"ff1ed760") NAMEPILLSCOLORS="Green";;
"ff47ae84") NAMEPILLSCOLORS="Light Green";;
"ffffa842") NAMEPILLSCOLORS="Yellow";;
"ffff8055") NAMEPILLSCOLORS="Orange";;
"ffa86bd5") NAMEPILLSCOLORS="Violet";;
esac
UIP -s "$MSGVU$COLORVARIANT$NAMEPILLSCOLORS ($PILLSCOLORS)"
MSGPCFSGA="$MSGNIS"
case $PILLSCOLORS in
"ffa86bd5") MSGPCFSGA="$MSGNUS";;
esac
UIP "$MSGVD No $MSGPCFSGA"
if KEYCHOOSE; then
PCFSGC=true
break
else
PCFSGC=false
fi
done
}

IPPH () {
if FINDER -aiopfmdph "$IPPSO/PillsHeight*.apk"; then
UIP -s "$MSGFI Pills Height, Remove?" "$MSGVU Yes" "$MSGVD No $MSGNUS"
if KEYCHOOSE; then
UIP "$MSGRMIP"
rm -f $IPPSO/PillsHeight*.apk
UIP "$MSGDRM"
fi
else
if [ $MIUI != true ] && [ "$ASDK" -ge 29 ]; then UCSA -ent "$MSGIP Pills Height $VIPPH?" "IPPHS" "$IDPHS"; fi
fi
}

IPPHS () {
UCSA -a "0dip 1dip 2dip 3dip 4dip 5dip 6dip 7dip 8dip 9dip 10dip" "Choose Pills Height" "Height:" "10dip"
if $UCSFNM; then
UIP "$MSGIAMM Pills Height"
UZTPN "PillsHeight"
if $MAGISK; then
PTMFI "$IDPHS" "$IDAPHS" "$MIPPSO"
cp -f $TMPDIR/$MODID/PillsHeight-$UCSFNMC.apk $MODPATH/$MODID$MIPPSO
MMPR -v "Pills Height" "$VIPPH" "$VCIPPH" "Pills Height - Set On: $UCSFNMC"
else
cp -f $TMPDIR/PillsHeight/PillsHeight-$UCSFNMC.apk $IPPSO
fi
SET_PERM_RM -msgdi
fi
}

IPNBVS () {
UCSA "$MSGIP Nav Bar Visibility Switcher $VIPNBVS?" "IPNBVSS" "$IDNBVS"
}

IPNBVSS () {
UIP -s "Choose Variant:" "$MSGVU Show" "$MSGVD Hide"
if KEYCHOOSE; then NBVSV="0"; MSGNBVS="Show"; else NBVSV="1"; MSGNBVS="Hide"; fi
UIP "$MSGIAMM Nav Bar Visibility Switcher"
if $MAGISK; then
PTMFI "$IDNBVS" "$IDANBVS"
echo "qemu.hw.mainkeys=$NBVSV" > $MODPATH/$MODID/system.prop
MMPR -v "Nav Bar Visibility Switcher" "$VIPNBVS" "$VCIPNBVS" "Nav Bar Visibility Switcher - Set On: $MSGNBVS"
else
BUCKUPER -bf "$IPSBP"
sed -i "/qemu.hw.mainkeys/d" $IPSBP
echo -e "\n# Nav Bar Visibility Switcher\nqemu.hw.mainkeys=$NBVSV" >> $IPSBP
fi
SET_PERM_RM -msgdi
}

IPBGD () {
if $MAGISK && [ $MIUI != true ] && [ "$ASDK" -ge 30 ]; then
UCSA "$MSGIP Back Gesture Disabler $VIPBGD?" "IPBGDS" "$IDBGDS"
fi
}

IPBGDS () {
UIP -s "Disable Left Gesture?" "$MSGVU Yes" "$MSGVD No $MSGNUS"
if KEYCHOOSE; then BGISL="0"; else BGISL="1"; fi
UIP -s "Disable Right Gesture?" "$MSGVU Yes" "$MSGVD No $MSGNUS"
if KEYCHOOSE; then BGISR="0"; else BGISR="1"; fi
UIP "$MSGIAMM Back Gesture Disabler"
PTMFI "$IDBGDS"
MSSERVICE "$SPSECURE back_gesture_inset_scale_left 1; $SPSECURE back_gesture_inset_scale_right 1" "$SPSECURE back_gesture_inset_scale_left $BGISL; $SPSECURE back_gesture_inset_scale_right $BGISR" "BGDS"
MMPR -v "Back Gesture Disabler" "$VIPBGD" "$VCIPBGD" "Back Gesture Disabler"; SET_PERM_RM -msgdi
}

IPFNS () {
if $MAGISK && [ $MIUI != true ] && [ "$ASDK" -ge 29 ]; then
UCSA "$MSGIP Force Navigation Switcher $VIPFNS?" "IPFNSS" "$IDFNS"
fi
}

IPFNSS () {
UIP -s "Choose Variant:" "$MSGVU Gestural" "$MSGVD Three Button"
if KEYCHOOSE; then FNSV="gestural"; else FNSV="threebutton"; fi
UIP "$MSGIAMM Force Navigation Switcher"
PTMFI "$IDFNS" "$IDAFNS"
MSSERVICE -s "cmd overlay enable com.android.internal.systemui.navbar.$FNSV"
MMPR -v "Force Navigation Switcher" "$VIPFNS" "$VCIPFNS" "Force Navigation Switcher - If The Navigation Selection Item Disappeared After $MSGIPI Other Launchers Or Forced Hiding NavBar"; SET_PERM_RM -msgdi
}

} #

