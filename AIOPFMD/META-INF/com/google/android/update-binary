IPVBHW="$IPVB/hw"; MIPSVBHW="$MIPSVB/hw"
IPVEDF="$IPVE/device_features"; MIPSVEDF="$MIPSV/device_features"
IPDA="/data/app"
IPDD="/data/data"
IPDS="/data/system"
IPDSDE="/data/system_de"
IPDSU="$IPDS/users/0"
IPDSUDE="/data/user_de"
