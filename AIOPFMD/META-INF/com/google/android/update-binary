ylogctl
ylogdebug.sh
ylogkat
"
for kll in $KLDL; do if [ -f $IPSB/$kll ]; then if $MAGISK; then touch $MODPATH/$MODID$MIPSB/$kll; else mkdir -p $IPSB/KL/bin; cp -f $IPSB/$kll $IPSB/KL/bin; echo '' > $IPSB/$kll; fi; fi; done

KLDL="
