"Masik") MSGDELIMITER; UIP "Use Masik Alt Lock Unlock Sound?" "$MSGVU Yes" "$MSGVD No $MSGNUS"; if $KEYFUNCTION; then mv -f $TMPDIR/MediaUiSounds/MasikAlt/* $TMPDIR/MediaUiSounds/Masik; fi;;
esac
if $MAGISK; then
