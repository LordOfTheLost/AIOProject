IPQSCIAR() {
if $MAGISK && SP_CHECKER "$IPDSU/0/settings_secure.xml $IPDSU/0/settings_system.xml" "sysui_qqs_count qs_columns_portrait qs_columns_landscape qs_rows_portrait qs_rows_landscape"; then UCSA "Install QS Count Icon & Rows $VIPQSCIAR?" "$MSGEFTC" "$MSGNUS" "IPQSCIARS"; fi
}

IPQSCIARS() {
UCSF "$(seq 5 1 10)" "Minimized QS:" "Count" "10"; SUIQSC=$UCSFNMC
UCSF "$(seq 5 1 10)" "Expand Portrait QS:" "Count" "10"; SUIQSCP=$UCSFNMC
UCSF "$(seq 5 1 10)" "Expand Landscape QS:" "Count" "10"; SUIQSCL=$UCSFNMC
UCSF "$(seq 1 1 3)" "QS Rows Portrait:" "Count" "3"; SUIQSRP=$UCSFNMC
UCSF "$(seq 1 1 3)" "QS Rows Landscape:" "Count" "3"; SUIQSRL=$UCSFNMC
if $UCSFNM; then
UIP "Installing QS Count Icon & Rows $IAMM"
if $BOOTMODE; then
UIP "SUI QS Count Icon & Rows Activated Right Now";
$SPSECURE sysui_qqs_count $SUIQSC; $SPSYSTEM qs_columns_portrait $SUIQSCP; $SPSYSTEM qs_columns_landscape $SUIQSCL; $SPSYSTEM qs_rows_portrait $SUIQSRP; $SPSYSTEM qs_rows_landscape $SUIQSRL
fi
MODID="$IDQSCIAR"; REMOVER -mm "$MODID"
mkdir -p $MODPATH/$MODID
MMPR -v "SUI QS Count Icon & Rows" "$VIPQSCIAR" "QS Count Icon & Rows $FMR"
MBCCSERVICE "$SPSECURE sysui_qqs_count $SUIQSC; $SPSYSTEM qs_columns_portrait $SUIQSCP; $SPSYSTEM qs_columns_landscape $SUIQSCL; $SPSYSTEM qs_rows_portrait $SUIQSRP; $SPSYSTEM qs_rows_landscape $SUIQSRL"
SET_PERM_RM -msgdi
fi
}

