if $MAGISK; then
PTMFI "$IDGBSTS" "$IDAGBSTS"
MMPR -v "Gboard Silk Theme Switcher" "$VIPGBSTS" "$VCIPGBSTS" "Gboard Silk Theme Switcher - Set On: $MSGGBSTS"
