PXMLNLS -u $ACONF '/configs/property[@name="aac_adts_offload_enabled"]' "true"
PXMLNLS -u $ACONF '/configs/property[@name="alac_offload_enabled"]' "true"
PXMLNLS -u $ACONF '/configs/property[@name="ape_offload_enabled"]' "true"
