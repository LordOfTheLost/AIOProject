IPSFW="$SYSTEM/framework"; MIPSFW="/system/framework"
IPSPO="$IPSSP/overlay"; MIPSPO="$MIPSSP/overlay"
IPSO="$SYSTEM/overlay"; MIPSO="/system/overlay"
