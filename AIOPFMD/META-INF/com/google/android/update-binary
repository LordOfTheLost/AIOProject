fi
}

IPVFXS() {
UZTPN "V4AFX"
mkdir -p $TMPDIR/V4AFX/ViPERN$MIPSVE $TMPDIR/V4AFX/ViPERN$MIPSVE $TMPDIR/V4AFX/ViPERN$MIPSPA $TMPDIR/V4AFX/ViPERN$MIPSEP
