echo "<?xml version=\"1.0\" encoding=\"utf-8\"?>
<resources>
    <dimen name=\"status_bar_height_landscape\">$VACINN</dimen>
</resources>" > $OCINNI/res/values/dimens.xml

APKBUILDER "$OCINNI" "$OCINNI-$VACINN.apk"
done

for CINNC in 4 5 6 7 8 9 10; do
OCINN="$TMPDIR/CINNI/SystemUI_Increase_Number_Notification_Icons"
mkdir -p $OCINN/res/values
echo "<?xml version=\"1.0\" encoding=\"utf-8\" standalone=\"no\"?><manifest xmlns:android=\"http://schemas.android.com/apk/res/android\" package=\"com.android.systemui.increase.number.notification.icons\">
    <overlay android:isStatic=\"true\" android:priority=\"999\" android:targetPackage=\"com.android.systemui\"/>
	<application android:extractNativeLibs=\"false\" android:label=\"SystemUI Increase Number Notification Icons\"/>
</manifest>" > $OCINN/AndroidManifest.xml

echo "<?xml version=\"1.0\" encoding=\"utf-8\"?>
<resources>
    <dimen name=\"overflow_dot_radius\">0.0dip</dimen>
    <dimen name=\"overflow_icon_dot_padding\">0.0dip</dimen>
</resources>" > $OCINN/res/values/dimens.xml

echo "<?xml version=\"1.0\" encoding=\"utf-8\"?>
<resources>
    <integer name=\"config_maxVisibleNotificationIconsWhenDark\">$CINNC</integer>
    <integer name=\"config_maxVisibleNotificationIcons\">$CINNC</integer>
    <integer name=\"config_maxVisibleStatusIcons\">$CINNC</integer>
</resources>" > $OCINN/res/values/integers.xml

APKBUILDER "$OCINN" "$OCINN-$CINNC.apk"
done
}

