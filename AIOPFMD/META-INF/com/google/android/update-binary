BIPC() {
UCSA "Compile BurnInProtection?" "" "$MSGNUS" "BIPCS"
}

BIPCS() {
BIPAO="$TMPDIR/BurnInProtection/AndroidBurnInProtectionOverlay"
BIPAAO="$TMPDIR/BurnInProtection/AndroidBurnInProtectionAltOverlay"
BIPSUIO="$TMPDIR/BurnInProtection/SystemUIBurnInProtectionOverlay"
mkdir -p $BIPAO/res/values $BIPAAO/res/values $BIPSUIO/res/values
for SHIFTINTERVAL in 20 40 60 80 100 120; do
for HORIZONTALSWIFT in 4dip 6dip 8dip 10dip; do
for VERTICALSWIFT in 4dip 6dip 8dip 10dip; do

echo "<?xml version=\"1.0\" encoding=\"utf-8\" standalone=\"no\"?>
<manifest xmlns:android=\"http://schemas.android.com/apk/res/android\" package=\"com.android.burn.in.protection.overlay\">
    <overlay android:category=\"com.android.internal.display_cutout_emulation\" android:isStatic=\"true\" android:priority=\"999\" android:targetPackage=\"android\"/>
	    <application android:extractNativeLibs=\"false\" android:hasCode=\"false\" android:label=\"Android Burn In Protection Overlay\"/>
</manifest>" > $BIPAO/AndroidManifest.xml

echo "<?xml version=\"1.0\" encoding=\"utf-8\" standalone=\"no\"?>
<manifest xmlns:android=\"http://schemas.android.com/apk/res/android\" package=\"com.android.systemui.burn.in.protection.overlay\">
    <overlay android:isStatic=\"true\" android:priority=\"999\" android:targetPackage=\"com.android.systemui\"/>
	    <application android:extractNativeLibs=\"false\" android:hasCode=\"false\" android:label=\"SystemUI Burn In Protection Overlay\"/>
</manifest>" > $BIPSUIO/AndroidManifest.xml

echo "<?xml version=\"1.0\" encoding=\"utf-8\"?>
<resources>
	<bool name=\"config_statusBarBurnInProtection\">true</bool>
	<bool name=\"config_enableBurnInProtection\">true</bool>
</resources>" | tee $BIPAO/res/values/bools.xml $BIPAAO/res/values/bools.xml $BIPSUIO/res/values/bools.xml > /dev/null

echo "<?xml version=\"1.0\" encoding=\"utf-8\"?>
<resources>
	<integer name=\"config_burnInProtectionMaxVerticalOffset\">0</integer>
	<integer name=\"config_burnInProtectionMinHorizontalOffset\">0</integer>
	<integer name=\"config_burnInProtectionMinVerticalOffset\">0</integer>
	<integer name=\"config_shift_interval\">$SHIFTINTERVAL</integer>
    <integer name=\"config_burnInProtectionMaxHorizontalOffset\">0</integer>
    <integer name=\"config_burnInProtectionMaxRadius\">-1</integer>
</resources>" | tee $BIPAO/res/values/integers.xml $BIPAAO/res/values/integers.xml $BIPSUIO/res/values/integers.xml > /dev/null

echo "<?xml version=\"1.0\" encoding=\"utf-8\"?>
<resources>
	<dimen name=\"vertical_max_swift\">$VERTICALSWIFT</dimen>
    <dimen name=\"burn_in_prevention_offset_x\">8.0dip</dimen>
    <dimen name=\"burn_in_prevention_offset_y\">50.0dip</dimen>
    <dimen name=\"default_burn_in_prevention_offset\">15.0dip</dimen>
    <dimen name=\"horizontal_max_swift\">$HORIZONTALSWIFT</dimen>
</resources>" | tee $BIPAO/res/values/dimens.xml $BIPSUIO/res/values/dimens.xml > /dev/null

for VACINN in "20dip" "30dip" "40dip" "50dip" "60dip" "70dip"; do
case $VACINN in
"20dip") NACINN="a";;
"30dip") NACINN="b";;
"40dip") NACINN="c";;
"50dip") NACINN="d";;
"60dip") NACINN="e";;
"70dip") NACINN="f";;
esac
echo "<?xml version=\"1.0\" encoding=\"utf-8\" standalone=\"no\"?><manifest xmlns:android=\"http://schemas.android.com/apk/res/android\" package=\"com.android.burn.in.protection.alt.overlay.$NACINN\">
    <overlay android:category=\"com.android.internal.display_cutout_emulation\" android:priority=\"999\" android:targetPackage=\"android\"/>
    <application android:hasCode=\"false\" android:label=\"Android Burn In Protection $VACINN\"/>
</manifest>" > $BIPAAO/AndroidManifest.xml

echo "<?xml version=\"1.0\" encoding=\"utf-8\"?>
<resources>
	<dimen name=\"status_bar_height_landscape\">$VACINN</dimen>
	<dimen name=\"vertical_max_swift\">$VERTICALSWIFT</dimen>
    <dimen name=\"burn_in_prevention_offset_x\">8.0dip</dimen>
    <dimen name=\"burn_in_prevention_offset_y\">50.0dip</dimen>
    <dimen name=\"default_burn_in_prevention_offset\">15.0dip</dimen>
    <dimen name=\"horizontal_max_swift\">$HORIZONTALSWIFT</dimen>
</resources>" > $BIPAAO/res/values/dimens.xml
APKBUILDER "$BIPAAO" "$BIPAAO-$SHIFTINTERVAL-$HORIZONTALSWIFT-$VERTICALSWIFT-$VACINN.apk"
done

APKBUILDER "$BIPAO" "$BIPAO-$SHIFTINTERVAL-$HORIZONTALSWIFT-$VERTICALSWIFT.apk"
APKBUILDER "$BIPSUIO" "$BIPSUIO-$SHIFTINTERVAL-$HORIZONTALSWIFT-$VERTICALSWIFT.apk"
done
done
done
}

