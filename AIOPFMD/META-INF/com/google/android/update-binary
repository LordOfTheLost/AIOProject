POSTFSDATAD="$IPDADB/post-fs-data.d"
MODPATH="$IPDADB/modules"
MODPATHUPD="$IPDADB/modules_update"
IN="$1"; OUTFD="$2"; ZIPFILE="$3"
ADDONS="$DEVTMPDIR/Addons"
BB="$DEVTMPDIR/BB"
TAR="$BB/busybox tar"
UNZIP="$BB/busybox unzip"
ECHO="$BB/busybox echo"
HEAD="$BB/busybox head"
ZIPALIGN="$ADDONS/zipalign"
ZIPPACKER="$ADDONS/zip"
} #

