PXMLNLS -u $ACONF '/configs/property[@name="use_xml_audio_policy_conf"]' "true"
PXMLNLS -u $ACONF '/configs/property[@name="voice_concurrency"]' "false "
PXMLNLS -u $ACONF '/configs/property[@name="afe_proxy_enabled"]' "false"
