PXMLNLS -u $MIX '/mixer/ctl[@name="BL DRE DRE Switch"]' "1"
PXMLNLS -s $MIX '/mixer/ctl[@name="BL DRE DRE Switch"]' "1"
PXMLNLS -u $MIX '/mixer/ctl[@name="RCV Boost Class-H Tracking Enable"]' "1"
