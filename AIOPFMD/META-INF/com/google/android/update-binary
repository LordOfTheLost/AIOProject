net.hspa.dns2=$TNetworkTweak
net.hsdpa.dns1=$ONetworkTweak
net.hsdpa.dns2=$TNetworkTweak
net.lte.dns1=$ONetworkTweak
net.lte.dns2=$TNetworkTweak
