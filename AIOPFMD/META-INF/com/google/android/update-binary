WCANFTP -sl; WCANFTP -ehap
UIP "$MSGDI"
}

IPAN() {
if $MAGISK && [ $MAGISKFIRSTTIME != true ] && $FINDAIOPFMDA; then
UCSA "Install Ainur Narsil $VIPAN?" "" "$MSGNUS" "IPANS"
fi
}

IPANS() {
UZTPN "AinurNarsil"; UNIVERSALUNZIP
}

IPNLS() {
if $MAGISK && $SNAPSOC; then
UCSA "Install NLSound $VIPNLS?" "$MSGEFTC" "$MSGNUS" "IPNLSS"
fi
}

IPNLSS() {
STEP1=false; STEP2=false; STEP3=false; STEP4=false; STEP5=false; STEP6=false; STEP7=false; STEP8=false; STEP9=false; STEP10=false; STEP11=false; STEP12=false; STEP13=false; STEP14=false; PATCH_MIXER_LITE=false
if find $SYSTEM $VENDOR -type f -name "audio_configs*.xml" >/dev/null; then
MSGDELIMITER; UIP "Disable Deep Buffer?" "This Option Disable Deep Buffer In Your Device" "If You Want More Low Frequencies Skip This Option" "$MSGVU Yes" "$MSGVD No $MSGNIS"
if $KEYFUNCTION; then STEP1=true; fi
fi
MSGDELIMITER; UIP "Improve Volume Levels?" "CONFIRMING THIS OPTION MAY HARM YOUR DEVICE!" "NLSound Team Is Not Responsible For Your Devices!" "Choose At Your Own Risk!" "$MSGVU Yes" "$MSGVD No $MSGNIS"
if $KEYFUNCTION; then STEP2=true; fi
MSGDELIMITER; UIP "Improve Microphone Levels?" "This Option Improving Microphone Volume Levels In Your Device" "$MSGVU Yes" "$MSGVD No $MSGNIS"
if $KEYFUNCTION; then STEP3=true; fi
MSGDELIMITER; UIP "Patching IIR?" "IIR Directly Affects The Final Sound Quality" "And It Is Recommended To Try The Version" "With And Without It, Choosing The One That You Like The Most" "$MSGVU Yes $IR" "$MSGVD No $MSGNIS"
if $KEYFUNCTION; then STEP4=true; fi
if [ -f $APIIXML ] || [ -f $APIEXML ] || [ -f $APIXML ]; then
MSGDELIMITER; UIP "Patching Audio Platform Files?" "Confirming This Option Will Allow The" "Module To Force 24-Bit Audio" "For Your Favorite Songs, As Well As Improve" "The Sound Quality During Video Recording" "$MSGVU Yes $IR" "$MSGVD No $MSGNIS"
if $KEYFUNCTION; then STEP5=true; fi
fi
MSGDELIMITER; UIP "Disable Companders?" "Companding - Method For Reducing The Effects" "Of Channels With A Limited Dynamic Range" "$MSGVU Yes" "$MSGVD No $MSGNIS"
if $KEYFUNCTION; then STEP6=true; fi
MSGDELIMITER; UIP "Configurating Interal Audio Codec?" "This Option Configuring Your Device's Internal Audio Codec" "$MSGVU Yes" "$MSGVD No $MSGNIS"
if $KEYFUNCTION; then STEP7=true; fi
if [ -d $IPSEDF ] || [ -d $IPVEDF ]; then
MSGDELIMITER; UIP "Patch device_features Files?" "Added More Stuff" "Enable HD Record In Camcorder" "Increase VoIP Recor Quality" "$MSGVU Yes" "$MSGVD No$MSGNIS"
if $KEYFUNCTION; then STEP8=true; fi
fi
MSGDELIMITER; UIP "Added New Dirac?" "This Option Added New Dirac In Your ROM" "$MSGVU Yes" "$MSGVD No $MSGNIS"
if $KEYFUNCTION; then STEP9=true; fi
MSGDELIMITER; UIP "Disable Useless Fluence?" "This Option Disables Useless Noise Reduction" "This Will Also Affect The" "Quality Of The Audio Recording" "$MSGVU Yes $IR" "$MSGVD No $MSGNIS"
if $KEYFUNCTION; then STEP10=true; fi
MSGDELIMITER; UIP "Install Patches In mixer_paths Files?" "A Large Set Of Universal Settings For Many Devices" "If You Encounter Problems After Installation" "Try Skipping This Option First" "$MSGVU Yes" "$MSGVD No $MSGNIS"
if $KEYFUNCTION; then
STEP11=true
MSGDELIMITER; UIP "What Option To Patch?" "$MSGVU Patch Light mixer_paths" "$MSGVD Patch Full mixer_paths"
if $KEYFUNCTION; then PATCH_MIXER_LITE=true; fi
fi
MSGDELIMITER; UIP "Improve Decoders & Encoders Audio?" "This Option Will Improve The Audio Processing" "Algorithms Of The Built-In Audio Decoders" "And Encoders" "$MSGVU Yes" "$MSGVD No $MSGNIS"
if $KEYFUNCTION; then STEP12=true; fi
MSGDELIMITER; UIP "Install Patches For Hi-Fi Audio?" "This Option Will Improve The Processing Of" "High-Pass Audio, As Well As Eliminate" " The Problem Of Audio Clipping" "$MSGVU Yes" "$MSGVD No $MSGNIS"
if $KEYFUNCTION; then STEP13=true; fi
