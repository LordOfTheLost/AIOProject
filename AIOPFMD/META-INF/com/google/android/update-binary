cp -f $TMPDIR/NLSound/tfa9894.cnt $MODPATH/$MODID$MIPSVF
if [ "$SD662" ] || [ "$SD665" ] || [ "$SD690" ] || [ "$SD710" ] || [ "$SD720G" ] || [ "$SD730" ] || [ "$SD765G" ] || [ "$SD820" ] || [ "$SD835" ] || [ "$SD845" ] || [ "$SD855" ] || [ "$SD865" ] || [ "$SD888" ]; then
HIFI=true; UIP "Device With Support Hi-Fi"
else
HIFI=false; UIP "Device Without Support Hi-Fi"
fi
echo "media.stagefright.audio.sink=128
af.fast.track.multiplier=2
vendor.audio_hal.period_multiplier=3 
audio.offload.buffer.size.kb=32
persist.mm.enable.prefetch=true
ro.af.client_heap_size_kbyte=7168
