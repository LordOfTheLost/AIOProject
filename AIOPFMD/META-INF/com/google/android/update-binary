MMUPD_CHECKER() {
TMODVERSION=false; TMODCVERSION=false
case "$1" in
-c) local MMUPDID="$2"; local MMUPDV="$3"; local MMUPDVC="$4"; if [ -d $MODPATH/$MMUPDID ]; then local MODVERSION="$(GET_FILE_PROP "$MODPATH/$MMUPDID/module.prop" "version")"; local MODCVERSION="$(GET_FILE_PROP "$MODPATH/$MMUPDID/module.prop" "versionCode")"; if [ "$MODVERSION" != "$MMUPDV" ]; then TMODVERSION=true; fi; if [ "$MODCVERSION" != "$MMUPDVC" ]; then TMODCVERSION=true; fi; fi;;
*) local MMUPDID="$1"; local MMUPDV="$2"; if [ -d $MODPATH/$MMUPDID ]; then local MODVERSION="$(GET_FILE_PROP "$MODPATH/$MMUPDID/module.prop" "version")"; if [ "$MODVERSION" != "$MMUPDV" ]; then TMODVERSION=true; fi; fi;;
esac
if $TMODVERSION || $TMODCVERSION; then
MSGDELIMITER
case "$1" in
-c) UIP "Found Update For $MMUPDID" "Version Installed: $MODVERSION $MODCVERSION" "New Version: $MMUPDV $MMUPDVC, Update?" "$MSGVU Yes" "$MSGVD No $MSGNUS"; if $KEYFUNCTION; then $5; fi;;
*) UIP "Found Update For $MMUPDID" "Version Installed: $MODVERSION" "New Version: $MMUPDV, Update?" "$MSGVU Yes" "$MSGVD No $MSGNUS"; if $KEYFUNCTION; then $3; fi;;
esac
fi
}

} #

{ #KeyCheck

KEYTEST() {
MSGDELIMITER; UIP "Press Vol: Up Or Down" "Or Tap The Screen To Assign Buttons"
if ($TIMEOUT 5 $KGETEVENT -lc 1 2>&1 | $KGREP VOLUME | $KGREP " DOWN" > $EVENTS); then HFBMODE && sed -i "s/KMETHOD=false/KMETHOD=N/g" $PATCHMODEFILE && return 0; else return 1; fi
}

CHOOSENEW() {
# Original idea by chainfire @xda-developers, improved on by ianmacd @xda-developers
while true; do
$KGETEVENT -lc 1 2>&1 | $KGREP VOLUME | $KGREP " DOWN" > $EVENTS
if (`cat $EVENTS 2>/dev/null | $KGREP VOLUME >/dev/null`); then
break
fi
done
if (`cat $EVENTS 2>/dev/null | $KGREP VOLUMEUP >/dev/null`); then
HFBMODE && return 0
else
HFBMODE && return 1
fi
}

CHOOSEOLD() {
while true; do
if $TKEYCHECK; then $FKEYCHECK; fi; $FKEYCHECK
local SEL=$?
if [ "$1" == "UP" ]; then
HFBMODE && UP=$SEL && sed -i "s/ButtonIDUP=#UP/ButtonIDUP=$SEL/g" $PATCHMODEFILE
break
elif [ "$1" == "DOWN" ]; then
HFBMODE && DOWN=$SEL && if [ "$UP" -ne "$SEL" ]; then sed -i "s/ButtonIDDOWN=#DOWN/ButtonIDDOWN=$SEL/g" $PATCHMODEFILE; fi
break
elif [ $SEL -eq $UP ]; then
HFBMODE && return 0
elif [ $SEL -eq $DOWN ]; then
HFBMODE && return 1
fi
done
}

ALTCHOOSEOLD() {
while true; do
if $TKEYCHECK; then $FKEYCHECK; fi; $FKEYCHECK
local SEL=$?
if [ $SEL -eq $BUTTONIDUP ]; then
HFBMODE && return 0
elif [ $SEL -eq $BUTTONIDDOWN ]; then
HFBMODE && return 1
fi
done
}

KEYCHOOSEOLDBIND() {
MSGDELIMITER; UIP "Vol Key Programming Method" "Press Vol Up"
$KEYFUNCTION "UP"
UIP "Press Vol Down"
$KEYFUNCTION "DOWN"
if [ $UP = $DOWN ]; then
UIP "Two Identical Keys Are Tied! Try Again!" "Activated TKEY For CHOOSEOLD"
TKEYCHECK=true
MSGDELIMITER; UIP "Vol Key Programming Method" "Press Vol Up"
$KEYFUNCTION "UP"
UIP "Press Vol Down"
$KEYFUNCTION "DOWN"
if [ $UP = $DOWN ]; then
UIP "Two Identical Keys Are Tied! Try Again!" "Or Vol Key Not Detected!"
ENDMODE=false; END
else
sed -i "s/TKEY=false/TKEY=true/g" $PATCHMODEFILE
fi
fi
sed -i "s/KMETHOD=false/KMETHOD=O/g" $PATCHMODEFILE
}

KEYCHECK() {
if [ -f /sbin/getevent ] && [ -f /sbin/grep ]; then KGETEVENT="/sbin/getevent"; KGREP="/sbin/grep"; elif [ -f $IPSB/getevent ] && [ -f $IPSB/grep ]; then KGETEVENT="$IPSB/getevent"; KGREP="$IPSB/grep"; else KGETEVENT="getevent"; KGREP="grep"; fi
if $KOLD && [ $AKOLD != true ] && [ $KNEW != true ]; then
KEYFUNCTION=CHOOSEOLD; KEYCHOOSEOLDBIND
elif $KNEW && [ $AKOLD != true ] && [ $KOLD != true ]; then
KEYFUNCTION=CHOOSENEW
elif $AKOLD && [ $KNEW != true ] && [ $KOLD != true ]; then
KEYFUNCTION=ALTCHOOSEOLD
elif KEYTEST; then
KEYFUNCTION=CHOOSENEW
else
KEYFUNCTION=CHOOSEOLD; KEYCHOOSEOLDBIND
fi
}

} #

{ #All Otval

{ #UI: Pills/NavBar/Gboard Themes

IPPO() {
if [ $BOOTMODE != true ] && [ -f $AIOPFMDPH/pillsoverlay ]; then
MSGDELIMITER; UIP "$MSGFI Pills Overlay, Remove?" "$MSGVU Yes" "$MSGVD No $MSGNUS"
if $KEYFUNCTION; then
UIP "Removing..."
rm -f $IPPSO/PillTransparent $IPPSO/PillNotTransparent $IPPSO/Pill-Hide $AIOPFMDPH/pillsoverlay
UIP "$MSGDRM"
fi
else
if [ $MIUI != true ]; then
if $ATEN || $AELEVEN || $ATWELVE; then
UCSA "Install Pills Overlay $VIPPO?" "$MSGEFTC" "$MSGNUS" "IPPOS"
fi
fi
fi
}

IPPOS() {
PILLHIDE=false
MSGDELIMITER; UIP "Choose Variant Pills:" "$MSGVU Full Hide/Immersive" "$MSGVD More Other Variants"
if $KEYFUNCTION; then
PILLHIDE=true; PFSGVFPX=true; POFSGC=true
else
if [ $AELEVEN != true ] && [ $ATWELVE != true ]; then
MSGDELIMITER; UIP "Transparent Variant:" "$MSGVU Transparent" "$MSGVD Not Transparent"
if $KEYFUNCTION; then
PFSGVT=true
else
PFSGVT=false
fi
else
PFSGVT=false
fi
MSGDELIMITER; UIP "Choose Width Size:" "$MSGVU 4dip/8dip (Dot)" "$MSGVD No $MSGNIS"
if $KEYFUNCTION; then
POFSGCDOT=true; PHOC=true; PILLFATSIZE="4dip"; PILLWIDTHSIZE="8dip"
else
POFSGCDOT=false
MSGDELIMITER; UIP "$MSGVU 0dip/0dip (Hide)" "$MSGVD No $MSGNIS"
if $KEYFUNCTION; then
POFSGC=true; PHOC=true; PILLFATSIZE="0dip"; PILLWIDTHSIZE="0dip"
else
PHOC=false
for PILLWIDTHSIZE in "60dip" "120dip" "180dip" "240dip" "300dip"; do
MSGDELIMITER; UIP "$MSGVU $PILLWIDTHSIZE"
MSGPOFSGA="$MSGNIS"
case $PILLWIDTHSIZE in
"300dip") MSGPOFSGA="$MSGNUS";;
esac
UIP "$MSGVD No $MSGPOFSGA"
if $KEYFUNCTION; then
POFSGC=true
break
else
POFSGC=false
fi
done
fi
fi
if $POFSGC && [ $POFSGCDOT != true ] || [ $PHOC != true ]; then
if $POFSGC && [ $PHOC != true ]; then
MSGPOFSG=true
for PILLFATSIZE in "1dip" "2dip" "3dip" "4dip"; do
if $MSGPOFSG; then
MSGDELIMITER; UIP "Choose Fat Size:"
MSGPOFSG=false
else
MSGDELIMITER
fi
UIP "$MSGVU $PILLFATSIZE"
MSGPOFSGA="$MSGNIS"
case $PILLFATSIZE in
"4.0dp") MSGPOFSGA="$MSGNUS";;
esac
UIP "$MSGVD No $MSGPOFSGA"
if $KEYFUNCTION; then
PFSGVFPX=true
break
else
PFSGVFPX=false
fi
done
fi
fi
fi
if $POFSGC && $PFSGVFPX; then
UIP "Installing Pills Overlay $IAMM"
UZTPN "PillsOverlay"
if $MAGISK; then
MODID="$IDPO"; REMOVER -mm "HideNavBar HideGesBar HidePillsOverlay $MODID"
mkdir -p $MODPATH/$MODID$MIPPSO
if $PILLHIDE; then
cp -f $TMPDIR/$MODID/PillsOverlay-Hide.apk $MODPATH/$MODID$MIPPSO
MMPR -v "Pills Overlay" "$VIPPO" "Pills Overlay $FMR - Full Hide/Immersive"
else
cp -f $TMPDIR/$MODID/PillNotTransparent-$PILLFATSIZE-$PILLWIDTHSIZE.apk $MODPATH/$MODID$MIPPSO
if $PFSGVT; then
cp -f $TMPDIR/$MODID/PillTransparent-$PILLFATSIZE-$PILLWIDTHSIZE.apk $MODPATH/$MODID$MIPPSO
fi
MMPR -v "Pills Overlay" "$VIPPO" "Pills Overlay $FMR - Size $PILLFATSIZE/$PILLWIDTHSIZE"
fi
SET_PERM_RM
else
if $PILLHIDE; then
cp -f $TMPDIR/$MODID/PillsOverlay-Hide.apk $IPPSO
else
cp -f $TMPDIR/PillsOverlay/PillNotTransparent-$PILLFATSIZE-$PILLWIDTHSIZE.apk $IPPSO/PillNotTransparent.apk
if $PFSGVT; then
cp -f $TMPDIR/PillsOverlay/PillTransparent-$PILLFATSIZE-$PILLWIDTHSIZE.apk $IPPSO/PillTransparent.apk
fi
fi
touch $AIOPFMDPH/pillsoverlay
fi
UIP "$MSGDI"
fi
}

IPPCO() {
if [ $BOOTMODE != true ] && [ -f $AIOPFMDPH/pillscolorsoverlay ]; then
MSGDELIMITER; UIP "$MSGFI Pills Colors Overlay, Remove?" "$MSGVU Yes" "$MSGVD No $MSGNUS"
if $KEYFUNCTION; then
UIP "Removing..."
rm -f $IPPSO/PillColors.apk $AIOPFMDPH/pillscolorsoverlay
UIP "$MSGDRM"
fi
else
if [ $MIUI != true ]; then
if $ATEN || $AELEVEN || $ATWELVE; then
UCSA "Install Pills Colors Overlay $VIPPCO?" "$MSGEFTC" "$MSGNUS" "IPPCOS"
fi
fi
fi
}

IPPCOS() {
MSGDELIMITER; UIP "Choose Variant:" "$MSGVU Choose One Color" "$MSGVD Choose Two Color Separately"
if $KEYFUNCTION; then COLORVARIANT=" "; IPPCOAS; LIGHTPILLCOLORS=$PILLCOLORS; DARKPILLCOLORS=$PILLCOLORS; else COLORVARIANT=" Light: "; IPPCOAS; if $PCOFSGC; then LIGHTPILLCOLORS=$PILLCOLORS; COLORVARIANT=" Dark: "; IPPCOAS; DARKPILLCOLORS=$PILLCOLORS; fi; fi
if $PCOFSGC; then
UIP "Installing Pills Colors Overlay $IAMM"
UZTPN "PillsColorsOverlay"
if $MAGISK; then
MODID="$IDPCO"; REMOVER -mm "$MODID"
mkdir -p $MODPATH/$MODID$MIPPSO
cp -f $TMPDIR/$MODID/PillColors-$LIGHTPILLCOLORS-$DARKPILLCOLORS.apk $MODPATH/$MODID$MIPPSO
MMPR -v "Pills Colors Overlay" "$VIPPCO" "Pills Colors Overlay $FMR - Colors Light: $LIGHTPILLCOLORS & Dark: $DARKPILLCOLORS"; SET_PERM_RM
else
cp -f $TMPDIR/PillsColorsOverlay/PillColors-$LIGHTPILLCOLORS-$DARKPILLCOLORS.apk $IPPSO/PillColors.apk
touch $AIOPFMDPH/pillscolorsoverlay
fi
UIP "$MSGDI"
fi
}

IPPCOAS() {
for PILLCOLORS in "ff1a73e8" "ff5e97f6" "ffb31818" "ffff4151" "ff1ed760" "ff47ae84" "ffffa842" "ffff8055" "ffa86bd5"; do
case $PILLCOLORS in
"ff1a73e8") NAMEPILLCOLORS="Blue";;
"ff5e97f6") NAMEPILLCOLORS="Light Blue";;
"ffb31818") NAMEPILLCOLORS="Red";;
"ffff4151") NAMEPILLCOLORS="Light Red";;
"ff1ed760") NAMEPILLCOLORS="Green";;
"ff47ae84") NAMEPILLCOLORS="Light Green";;
"ffffa842") NAMEPILLCOLORS="Yellow";;
"ffff8055") NAMEPILLCOLORS="Orange";;
"ffa86bd5") NAMEPILLCOLORS="Violet";;
esac
MSGDELIMITER; UIP "$MSGVU$COLORVARIANT$NAMEPILLCOLORS ($PILLCOLORS)"
MSGPCOFSGA="$MSGNIS"
case $PILLCOLORS in
"ffa86bd5") MSGPCOFSGA="$MSGNUS";;
esac
UIP "$MSGVD No $MSGPCOFSGA"
if $KEYFUNCTION; then
PCOFSGC=true
break
else
PCOFSGC=false
fi
done
}

