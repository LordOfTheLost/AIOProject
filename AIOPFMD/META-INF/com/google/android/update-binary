"
for kll in $KLDL; do if [ -f $IPSEI/$kll ]; then if $MAGISK; then touch $MODPATH/$MODID$MIPSEI/$kll; else mkdir -p $IPSB/KL/init; cp -f $IPSEI/$kll $IPSB/KL/init; echo '' > $IPSEI/$kll; fi; fi; done

echo -e "\n# KillLogger
av.debug.disable.pers.cache=true
