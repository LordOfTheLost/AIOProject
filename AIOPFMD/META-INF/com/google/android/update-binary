UIP "Google Pixel Charging Animation"
cp -rf $AIOPFMDPH/res $IPSSPE
rm -rf $AIOPFMDPH/res
UIP "$MSGUD"
fi
else
if $FINDAIOPFMDA; then
UCSA "Install Google Pixel Charging Animation?" "" "$MSGNUS" "IGPCAS"
fi
fi
fi
}

