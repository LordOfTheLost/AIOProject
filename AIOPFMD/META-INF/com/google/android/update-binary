IPUSBSUCS () {
UCSA -a "MTP Tethering MIDI PTP" "USB Config" "Config:" "PTP"
if $UCSFNM; then
UIP "Installing USB Screen Unlocked Config $IAMM"
case $UCSFNMC in "MTP") USBSUC="mtp";; "Tethering") USBSUC="rndis";; "MIDI") USBSUC="midi";; "PTP") USBSUC="ptp";; esac
if $MAGISK; then
PTMFI -nmp "$IDUSBSUC"
MSSERVICE -s "echo \"<?xml version='1.0' encoding='utf-8' standalone='yes' ?>\n<map>\n    <string name=\\\"usb-screen-unlocked-config-0\\\">$USBSUC</string>\n</map>\" > $IPDSDE/0/UsbDeviceManagerPrefs.xml"
MMPR -v "USB Screen Unlocked Config" "$VIPUSBSUC" "USB Screen Unlocked Config Set On: $UCSFNMC"
fi
echo -e "<?xml version='1.0' encoding='utf-8' standalone='yes' ?>\n<map>\n    <string name=\"usb-screen-unlocked-config-0\">$USBSUC</string>\n</map>" > $IPDSDE/0/UsbDeviceManagerPrefs.xml
SET_PERM_RM -msgdi
fi
}

IPFULIRC () {
if CHECKER "-device" "dipper" || CHECKER "-device" "beryllium"; then
if FINDER -aiopbak "$IPPVO/FaceUnlockService__auto_generated_rro_vendor.apk"; then
UIP -s "$MSGFI Face UnLock IR Changer, Remove?" "$MSGVU Yes" "$MSGVD No $MSGNUS"
if KEYCHOOSE; then
UIP "Removing..."
BUCKUPER -r "$IPPVO/FaceUnlockService__auto_generated_rro_vendor.apk"
UIP "$MSGDRM"
fi
else
if [ -f $IPSEP/com.motorola.faceunlock.xml ] || [ -f $IPSEP/privapp-permissions-org.pixelexperience.faceunlock.xml ] || [ -f $IPSEP/privapp-permissions-com.crdroid.faceunlock.xml ] || [ -f $IPSEP/privapp-permissions-com.spark.faceunlock.xml ] || [ -f $IPSEP/privapp-permissions-com.aospextended.faceunlock.xml ] || [ -f $IPSEP/privapp_whitelist_co.aospa.sense.xml ]; then
UCSA "$MSGIP Face UnLock IR Changer $VIPFULIRC?" "IPFULIRCS" "$IDFULIRC"
fi
fi
fi
}

IPFULIRCS () {
UIP -s "Choose Variant:" "$MSGVU Enable" "$MSGVD Disable"
if KEYCHOOSE; then SWITCHIR="5"; MSGSWITCHIR="Enable"; else SWITCHIR=""; MSGSWITCHIR="Disable"; fi
UIP "Installing Face UnLock IR Changer $IAMM"
UZTPN "FULIRC"
if [ -f $IPSEP/com.motorola.faceunlock.xml ]; then
FUPNXML="com.motorola.faceunlock"
elif [ -f $IPSEP/privapp-permissions-org.pixelexperience.faceunlock.xml ]; then
FUPNXML="org.pixelexperience.faceunlock"
elif [ -f $IPSEP/privapp-permissions-com.crdroid.faceunlock.xml ]; then
FUPNXML="com.crdroid.faceunlock"
elif [ -f $IPSEP/privapp-permissions-com.spark.faceunlock.xml ]; then
FUPNXML="com.spark.faceunlock"
elif [ -f $IPSEP/privapp-permissions-com.aospextended.faceunlock.xml ]; then
FUPNXML="com.aospextended.faceunlock"
elif [ -f $IPSEP/privapp_whitelist_co.aospa.sense.xml ]; then
FUPNXML="co.aospa.sense"
fi
if $MAGISK; then
PTMFI -nmp "$IDFULIRC"
echo -e "vendor.camera.aux.packagelist=com.android.camera,org.codeaurora.snapcam,org.lineageos.aperture,$FUPNXML\nro.face_unlock_service.enabled=true" > $MODPATH/$MODID/system.prop
if [ -f $IPSEP/com.motorola.faceunlock.xml ]; then
echo -e "\nro.face.moto_unlock_service.cam_id=$SWITCHIR" > $MODPATH/$MODID/system.prop
elif [ -f $IPSEP/privapp_whitelist_co.aospa.sense.xml ]; then
echo -e "\n# Face UnLock IR Changer\nvendor.camera.aux.packagelist=com.android.camera,org.codeaurora.snapcam,org.lineageos.aperture,$FUPNXML\nro.face.sense_service.camera_id=$SWITCHIR" > $MODPATH/$MODID/system.prop
else
mkdir -p $MODPATH/$MODID$MIPPVO
cp -f $TMPDIR/FULIRC/FaceUnlockService-$FUPNXML-$MSGSWITCHIR.apk $MODPATH/$MODID$MIPPVO/FaceUnlockService__auto_generated_rro_vendor.apk
fi
MMPR -v "Face UnLock IR Changer" "$VIPFULIRC" "Face UnLock IR Changer For MI8/POCOF1 - Set On: $MSGSWITCHIR"
else
BUCKUPER -bf "$IPSBP"
echo -e "\n# Face UnLock IR Changer\nvendor.camera.aux.packagelist=com.android.camera,org.codeaurora.snapcam,org.lineageos.aperture,$FUPNXML\nro.face_unlock_service.enabled=true" >> $IPSBP
if [ -f $IPSEP/com.motorola.faceunlock.xml ]; then
echo -e "\n# Face UnLock IR Changer\nro.face.moto_unlock_service.cam_id=$SWITCHIR" >> $IPSBP
elif [ -f $IPSEP/privapp_whitelist_co.aospa.sense.xml ]; then
echo -e "\n# Face UnLock IR Changer\nvendor.camera.aux.packagelist=com.android.camera,org.codeaurora.snapcam,org.lineageos.aperture,$FUPNXML\nro.face.sense_service.camera_id=$SWITCHIR" >> $IPSBP
else
BUCKUPER -s "$IPPVO/FaceUnlockService__auto_generated_rro_vendor.apk"
cp -f $TMPDIR/FULIRC/FaceUnlockService-$FUPNXML-$MSGSWITCHIR.apk $IPPVO/FaceUnlockService__auto_generated_rro_vendor.apk
fi
fi
SET_PERM_RM -msgdi
WCANFTP "-sl"
}

