rm -f $IPPSO/QRCodeScannerOnLockScreen*.apk
UIP "$MSGDRM"
fi
else
if [ $MIUI != true ] && [ "$ASDK" -eq 33 ]; then UCSA -ent "$MSGIP QR Code Scanner On Lock Screen $VIPQRCSOLS?" "IPQRCSOLSS" "$IDQRCSOLS"; fi
fi
}

IPQRCSOLSS () {
UIP -s "Choose Variant:" "$MSGVU Enable" "$MSGVD Disable"
if KEYCHOOSE; then QRCSOLSV="Enable"; QRCSOLSVO="1"; QRCSOLSVT="0"; else QRCSOLSV="Disable"; QRCSOLSVO="0"; QRCSOLSVT="1"; fi
UIP "$MSGIAMM QR Code Scanner On Lock Screen"
UZTPN "QRCSOLS"
if $MAGISK; then
PTMFI "$IDQRCSOLS" "$IDAQRCSOLS" "$MIPPSO"
cp -f $TMPDIR/QRCSOLS/QRCodeScannerOnLockScreen-$QRCSOLSV.apk $MODPATH/$MODID$MIPPSO
MSSERVICE "$SPSECURE lock_screen_show_qr_code_scanner $QRCSOLSVT" "$SPSECURE lock_screen_show_qr_code_scanner $QRCSOLSVO" "QRCSOLS"
MMPR -v "QR Code Scanner On Lock Screen" "$VIPQRCSOLS" "$VCIPQRCSOLS" "QR Code Scanner On Lock Screen - Set On: $QRCSOLSV"
else
cp -f $TMPDIR/QRCSOLS/QRCodeScannerOnLockScreen-$QRCSOLSV.apk $IPPSO
fi
SET_PERM_RM -msgdi
}

IPLSUS () {
if FINDER -aiopfmdph "$IPPSO/LockScreenUserSwitcher*.apk"; then
UIP -s "$MSGFI Lock Screen User Switcher, Remove?" "$MSGVU Yes" "$MSGVD No $MSGNUS"
if KEYCHOOSE; then
UIP "$MSGRMIP"
rm -f $IPPSO/LockScreenUserSwitcher*.apk
UIP "$MSGDRM"
fi
else
if [ $MIUI != true ] && [ "$ASDK" -ge 33 ]; then UCSA -ent "$MSGIP Lock Screen User Switcher $VIPLSUS?" "IPLSUSS" "$IDLSUS"; fi
fi
}

IPLSUSS () {
UIP -s "Choose Variant:" "$MSGVU Enable" "$MSGVD Disable"
if KEYCHOOSE; then LSUSV="Enable"; else LSUSV="Disable"; fi
