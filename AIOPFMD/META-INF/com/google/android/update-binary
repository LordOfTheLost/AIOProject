for LAUNCHERPNAME in "com.android.launcher3" "ch.deletescape.lawnchair.ci" "app.lawnchair" "net.oneplus.launcher" "com.sec.android.app.launcher" "com.google.android.apps.nexuslauncher" "com.asus.launcher"; do
case $LAUNCHERPNAME in
"com.android.launcher3") LAUNCHERPATH="$ORSLL3"; LAUNCHERNAME="Launcher3";;
"ch.deletescape.lawnchair.ci") LAUNCHERPATH="$ORSLLQ"; LAUNCHERNAME="Lawnchair Q";;
"app.lawnchair") LAUNCHERPATH="$ORSLLR"; LAUNCHERNAME="Lawnchair R";;
"net.oneplus.launcher") LAUNCHERPATH="$ORSLOP"; LAUNCHERNAME="OnePlus Launcher";;
"com.sec.android.app.launcher") LAUNCHERPATH="$ORSLOUI"; LAUNCHERNAME="OneUI Launcher";;
"com.google.android.apps.nexuslauncher") LAUNCHERPATH="$ORSLP"; LAUNCHERNAME="Pixel Launcher";;
"com.asus.launcher") LAUNCHERPATH="$ORSLA"; LAUNCHERNAME="Asus Launcher";;
esac
echo "<?xml version=\"1.0\" encoding=\"utf-8\" standalone=\"no\"?><manifest xmlns:android=\"http://schemas.android.com/apk/res/android\" package=\"$LAUNCHERPNAME.roundedui\">
    <overlay android:isStatic=\"true\" android:priority=\"999\" android:targetPackage=\"$LAUNCHERPNAME\" android:label=\"$LAUNCHERNAME Rounded - $ROUNDEDSIZE\"/>
</manifest>" > $LAUNCHERPATH/AndroidManifest.xml

echo -e "<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<resources>\n$ROUNDEDUISTRING\n$ROUNDEDLSTRING\n</resources>" | tee $ORSLL3/res/values/dimens.xml $ORSLLQ/res/values/dimens.xml $ORSLLR/res/values/dimens.xml $ORSLOP/res/values/dimens.xml $ORSLOUI/res/values/dimens.xml $ORSLP/res/values/dimens.xml $ORSLA/res/values/dimens.xml > /dev/null
done
APKBUILDER "$ORSLL3" "$ORSLL3-$ROUNDEDSIZE.apk"
APKBUILDER "$ORSLLQ" "$ORSLLQ-$ROUNDEDSIZE.apk"
APKBUILDER "$ORSLLR" "$ORSLLR-$ROUNDEDSIZE.apk"
APKBUILDER "$ORSLOP" "$ORSLOP-$ROUNDEDSIZE.apk"
APKBUILDER "$ORSLOUI" "$ORSLOUI-$ROUNDEDSIZE.apk"
APKBUILDER "$ORSLP" "$ORSLP-$ROUNDEDSIZE.apk"
APKBUILDER "$ORSLA" "$ORSLA-$ROUNDEDSIZE.apk"
done
}

PillsOverlayC() {
UCSA "Pills Overlay Compile" "" "$MSGNUS" "PillsOverlayCS"
}

PillsOverlayCS() {

for PILLFATSIZE in "0dip" "1dip" "2dip" "3dip" "4dip"; do
for PILLWIDTHSIZE in "0dip" "60dip" "120dip" "180dip" "240dip" "300dip" "8dip"; do
OPTO="$TMPDIR/PillsOverlay/PillTransparent"
OPNTO="$TMPDIR/PillsOverlay/PillNotTransparent"
mkdir -p $OPTO/res/values $OPNTO/res/values
echo "<?xml version=\"1.0\" encoding=\"utf-8\" standalone=\"no\"?><manifest xmlns:android=\"http://schemas.android.com/apk/res/android\" package=\"com.android.internal.systemui.navbar.gestural\">
    <overlay android:category=\"com.android.internal.navigation_bar_mode\" android:isStatic=\"true\" android:priority=\"999\" android:targetPackage=\"android\"/>
    <application android:extractNativeLibs=\"false\" android:hasCode=\"false\" android:label=\"Pill Transparent - $PILLFATSIZE/$PILLWIDTHSIZE\"/>
</manifest>" > $OPTO/AndroidManifest.xml

echo "<?xml version=\"1.0\" encoding=\"utf-8\"?>
<resources>
    <bool name=\"config_allowSeamlessRotationDespiteNavBarMoving\">true</bool>
    <bool name=\"config_navBarAlwaysShowOnSideEdgeGesture\">true</bool>
    <bool name=\"config_navBarCanMove\">false</bool>
    <bool name=\"config_navBarNeedsScrim\">false</bool>
    <bool name=\"config_navBarTapThrough\">true</bool>
</resources>" | tee $OPTO/res/values/bools.xml $OPNTO/res/values/bools.xml > /dev/null

echo "<?xml version=\"1.0\" encoding=\"utf-8\"?>
<resources>
    <dimen name=\"config_backGestureInset\">24.0dip</dimen>
    <dimen name=\"navigation_bar_frame_height\">48.0dip</dimen>
    <dimen name=\"navigation_bar_gesture_height\">32.0dip</dimen>
    <dimen name=\"navigation_bar_height\">0.0dip</dimen>
    <dimen name=\"navigation_bar_height_landscape\">0.0dip</dimen>
    <dimen name=\"navigation_bar_width\">0.0dip</dimen>
    <dimen name=\"navigation_handle_horizontal_margin\">30.0dip</dimen>
    <dimen name=\"navigation_handle_radius\">$PILLFATSIZE</dimen>
    <dimen name=\"navigation_handle_sample_horizontal_margin\">10.0dip</dimen>
    <dimen name=\"navigation_home_handle_width\">$PILLWIDTHSIZE</dimen>
    <dimen name=\"navigation_home_handle_width_land\">305.0dip</dimen>
</resources>" | tee $OPTO/res/values/dimens.xml $OPNTO/res/values/dimens.xml > /dev/null

echo "<?xml version=\"1.0\" encoding=\"utf-8\"?>
<resources>
    <integer name=\"config_navBarInteractionMode\">2</integer>
    <integer name=\"config_navBarOpacityMode\">2</integer>
</resources>" | tee $OPTO/res/values/integers.xml $OPNTO/res/values/integers.xml > /dev/null

echo "<?xml version=\"1.0\" encoding=\"utf-8\" standalone=\"no\"?><manifest xmlns:android=\"http://schemas.android.com/apk/res/android\" package=\"com.android.internal.systemui.pill.mod\">
    <overlay android:category=\"com.android.internal.navigation_bar_mode\" android:isStatic=\"true\" android:priority=\"999\" android:targetPackage=\"com.android.systemui\"/>
    <application android:extractNativeLibs=\"false\" android:hasCode=\"false\" android:label=\"Pill Not Transparent - $PILLFATSIZE/$PILLWIDTHSIZE\"/>
</manifest>" > $OPNTO/AndroidManifest.xml

APKBUILDER "$OPTO" "$TMPDIR/PillsOverlay/PillTransparent/PillTransparent-$PILLFATSIZE-$PILLWIDTHSIZE.apk"
APKBUILDER "$OPNTO" "$TMPDIR/PillsOverlay/PillNotTransparent/PillNotTransparent-$PILLFATSIZE-$PILLWIDTHSIZE.apk"
done
done

OPFH="$TMPDIR/PillsOverlay/PillsOverlay"
mkdir -p $OPFH/res/values
echo "<?xml version=\"1.0\" encoding=\"utf-8\" standalone=\"no\"?><manifest xmlns:android=\"http://schemas.android.com/apk/res/android\" package=\"com.android.internal.systemui.pill.mod\">
    <overlay android:category=\"com.android.internal.navigation_bar_mode\" android:isStatic=\"true\" android:priority=\"999\" android:targetPackage=\"com.android.systemui\"/>
    <application android:extractNativeLibs=\"false\" android:hasCode=\"false\" android:label=\"Pill Hide\"/>
</manifest>" > $OPFH/AndroidManifest.xml

echo "<?xml version=\"1.0\" encoding=\"utf-8\"?>
<resources>
    <dimen name=\"navigation_handle_horizontal_margin\">0.0dip</dimen>
    <dimen name=\"navigation_handle_radius\">0.0dip</dimen>
    <dimen name=\"navigation_handle_sample_horizontal_margin\">0.0dip</dimen>
    <dimen name=\"navigation_home_handle_width\">0.0dip</dimen>
    <dimen name=\"navigation_home_handle_width_land\">0.0dip</dimen>
</resources>" > $OPFH/res/values/bools.xml

APKBUILDER "$OPFH" "$TMPDIR/PillsOverlay/PillsOverlay/PillsOverlay-Hide.apk"

for LIGHTPILLCOLORS in "ff1a73e8" "ffb31818" "ff1ed760" "ffffa842" "ffff8055" "ffa86bd5" "ffff4151" "ff5e97f6" "ff47ae84"; do
for DARKPILLCOLORS in "ff1a73e8" "ffb31818" "ff1ed760" "ffffa842" "ffff8055" "ffa86bd5" "ffff4151" "ff5e97f6" "ff47ae84"; do
OPCO="$TMPDIR/PillsOverlay/PillColors"
mkdir -p $OPCO/res/values
echo "<?xml version=\"1.0\" encoding=\"utf-8\" standalone=\"no\"?><manifest xmlns:android=\"http://schemas.android.com/apk/res/android\" package=\"com.android.internal.systemui.pill.colors.mod\">
    <overlay android:isStatic=\"true\" android:priority=\"999\" android:targetPackage=\"com.android.systemui\"/>
    <application android:extractNativeLibs=\"false\" android:hasCode=\"false\" android:label=\"Pill Colors - $LIGHTPILLCOLORS-$DARKPILLCOLORS\"/>
</manifest>" > $OPCO/AndroidManifest.xml

echo "<?xml version=\"1.0\" encoding=\"utf-8\"?>
<resources>
	<color name=\"navigation_bar_home_handle_light_color\">#$LIGHTPILLCOLORS</color>
    <color name=\"navigation_bar_home_handle_dark_color\">#$DARKPILLCOLORS</color>
</resources>" > $OPCO/res/values/colors.xml

APKBUILDER "$OPCO" "$TMPDIR/PillsOverlay/PillColors/PillColors-$LIGHTPILLCOLORS-$DARKPILLCOLORS.apk"
done
done

for PILLHEIGHTSIZE in "0dip" "1dip" "2dip" "3dip" "4dip" "5dip" "6dip" "7dip" "8dip" "9dip" "10dip"; do
OPHO="$TMPDIR/PillsOverlay/PillHeight"
mkdir -p $OPHO/res/values
echo "<?xml version=\"1.0\" encoding=\"utf-8\" standalone=\"no\"?><manifest xmlns:android=\"http://schemas.android.com/apk/res/android\" package=\"com.android.internal.systemui.pill.height.mod\">
    <overlay android:category=\"com.android.internal.margin.navigation_bar_mode\" android:isStatic=\"true\" android:priority=\"999\" android:targetPackage=\"com.android.systemui\"/>
    <application android:extractNativeLibs=\"false\" android:hasCode=\"false\" android:label=\"Pill Height - $PILLHEIGHTSIZE\"/>
</manifest>" > $OPHO/AndroidManifest.xml

echo "<?xml version=\"1.0\" encoding=\"utf-8\"?>
<resources>
    <dimen name=\"navigation_handle_bottom\">$PILLHEIGHTSIZE</dimen>
</resources>" > $OPHO/res/values/dimens.xml

APKBUILDER "$OPHO" "$TMPDIR/PillsOverlay/PillHeight/PillHeight-$PILLHEIGHTSIZE.apk"
done
}

CINNIC() {
