IPMUC () {
if [ $MIUI != true ] && CHECKER -sp "$XMLGLOBAL" "guest_user_enabled"; then UCSA "$MSGIP Multi Users Changer $VIPMUC?" "IPMUCS" "$IDMUC"; fi
}

IPMUCS () {
UIP -s "Enable Or Disable Multi Users?" "$MSGVU Enable" "$MSGVD Disable"
if KEYCHOOSE; then
MUCSFC="1"; MUCSSC="0"; MUCFWMU="10"; MUCS="Enable"
else
MUCSFC="0"; MUCSSC="1"; MUCFWMU="1"; MUCS="Disable"
fi
UIP "Installing Multi Users Changer $IAMM"
if $MAGISK; then
PTMFI -np "$IDMUC" "SingleUserMod SingleUserEnabler DisableMultiUsers"
MSSERVICE "$SPGLOBAL guest_user_enabled $MUCSSC" "$SPGLOBAL guest_user_enabled $MUCSFC" "MUC"
echo -e "fw.max_users=$MUCFWMU\nfw.show_multiuserui=$MUCSFC" > $MODPATH/$MODID/system.prop
MMPR -v "Multi Users Changer" "$VIPMUC" "Multi Users Changer - Set On: $MUCS"
else
BUCKUPER -bf "$IPSBP"
echo -e "\n# Multi Users Changer\nfw.max_users=$MUCFWMU\nfw.show_multiuserui=$MUCSFC" >> $IPSBP
fi
SET_PERM_RM -msgdi
}

