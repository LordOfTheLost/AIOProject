BB="$ALTTMPDIR/BB"
EVENTS="$BB/events"
UNTARXZ="$BB/tar"
UNZIP="$BB/unzip"
TIMEOUT="$BB/timeout"
ECHO="$BB/echo"
HEAD="$BB/head"
ZIPALIGN="$AADDONS/zipalign"
ZIPPACKER="$AADDONS/zip"
} #

