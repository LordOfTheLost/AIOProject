MPXML="$(find $SYSTEM $VENDOR -type f -name "media_profiles*.xml")"
MCXML="$(find $SYSTEM $VENDOR -type f -name "media_codecs.xml")"
WCNSQCCFGI="$(find $SYSTEM $VENDOR -type f -name "WCNSS_qcom_cfg.ini")"
